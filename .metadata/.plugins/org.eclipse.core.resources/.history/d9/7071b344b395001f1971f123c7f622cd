package com.sepoct;

public class rec1 {
	
	public static void print_num(int num) {
		if(num==0) {
			System.out.println(num);
		}
		print_num(num-1);
		System.out.println(num);
	}

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int num = 5;
		print_num(num);
		
	}

}
