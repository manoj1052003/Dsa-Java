package com.jan26;

public class class7patt5 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int a = 16;
		int b = 32;
		int hcf = 0;
		int sm = (a<b)?a:b;
		for(int i=1; i<=sm; i++) {
			if(a%i==0 && b%i==0) {
				hcf = i;
			}
		}
		System.out.println("lcm " + (a*b)/hcf);
	}

}
