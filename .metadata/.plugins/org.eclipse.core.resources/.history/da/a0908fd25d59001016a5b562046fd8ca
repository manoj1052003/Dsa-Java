package com.basics;

public class Class2 {

	public static void main(String[] args) {
		int [] arr = {39,17,6,7};
		int max1 = 0;
		int max2 = 0;
		if(arr[0]>arr[1]) {
			max1 = arr[0];
			max2 = arr[1];
		}
		else {
			max1 = arr[1];
			max2 = arr[0];
		}
//		int max1 = arr[0];
//		int max2 = arr[1];
		for(int i=0; i<arr.length; i++) {
			if(arr[i]>max1) {
				max2= max1;
				max1 = arr[i];
			}
		}
		System.out.println(max2);
	}
}
