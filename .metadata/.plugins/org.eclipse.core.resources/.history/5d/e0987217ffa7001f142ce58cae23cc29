package com.oct21CSR;

public class basicclass3 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int i = 1;
		do {
			System.out.println("hi");
			i++;
		}
		while(i<=6);
		
		System.out.println("-------------");
		
		int k = 1;
		while(k<=6) {
			System.out.println("hello");
			k++;
		}
	}

}
