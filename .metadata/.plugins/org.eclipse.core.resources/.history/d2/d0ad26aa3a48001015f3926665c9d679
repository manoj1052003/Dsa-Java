package com.numbers;

import java.util.Arrays;

public class classmeth {
	
	public static void print_fact(int n) {
		int sum = 0;
		for(int i=1; i<n; i++) {
			if(n%i==0) {
				sum = sum + i;
			}
		}
	}

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int n = 6;
		print_fact(n);
			
	}
}
