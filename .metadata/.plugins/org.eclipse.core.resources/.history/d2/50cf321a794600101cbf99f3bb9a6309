package com.opr;

public class Pattern3 {
	
	public static void main(String[] args) {
		int rows = 5;
		for(int i=1; i<=rows; i++) {
			for(int j=1; j<=rows; j++) {
				if(i==1 || i==rows || j==1 || j==rows || i == j || rows+1-i == j || (rows+1)/2 == i || (rows+1)/2 == j) {
					System.out.print("*"+" ");
				}
				else {
					System.out.print(" "+" ");
				}
			}
			System.out.println();
		}
	}

}
