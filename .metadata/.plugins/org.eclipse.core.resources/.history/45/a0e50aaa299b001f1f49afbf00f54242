package com.sepoct;

public class num2 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int num = 123;
		int res = 0;
		int temp = num;
		while(num!=0) {
			int rem = num % 10;
			res = (res * 10) + rem;
			num = num / 10;
		}
		System.out.println(num);
		if(res==num) {
			System.out.println("pali");
		}
		else {
			System.out.println("not");
		}
	}

}
