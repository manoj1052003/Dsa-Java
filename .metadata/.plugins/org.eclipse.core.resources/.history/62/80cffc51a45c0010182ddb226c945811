package com.basic;

public class Secondcls {
	
	public static void main(String[] args) {
		int rows = 7;
		for(int i=1; i<=rows; i++) {
			for(int j=1; j<=rows; j++) {
				if(i==1 || j==1 || i==rows || j== rows || ( i==rows/2+1 && j>=rows/2+1)) {
					System.out.print("*"+" ");
				}
				else {
					System.out.print(" "+" ");
				}
			}
			System.out.println();
		}
		
		
	}

}
