package oct9;

public class class6 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int rows = 5;
		for(int i=1; i<=rows; i++) {//3
			for(int j =1; j<=rows-i; j++) {//1<=3
				System.out.print(" "+" ");
			}
			for(int j=1; j<=i; j++) { //1<=3
				System.out.print(j+" ");
			}
			for(int j =i-1; j>=1; j--) {//1<=2
				System.out.print(j+" ");
			}
			System.out.println();
		}
	}

}
