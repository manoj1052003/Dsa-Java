package com.basic;

import java.util.Arrays;

public class Logicalopr {
	

	public static void main(String[] args) {
		String s = "india is my country";
		String arr [] = s.split(" ");
		System.out.println(Arrays.toString(arr));
		for(int i=arr.length-1; i>=0; i--) {
			System.out.print(arr[i]+" ");
		}
		
	}

}
