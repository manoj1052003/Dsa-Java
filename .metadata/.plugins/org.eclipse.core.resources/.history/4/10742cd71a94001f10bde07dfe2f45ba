package workshop;
import java.util.*;
public class day7 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int [] arr = {12,23,34,45,56,67};
		int ele = 45;
		int ind = -1;
		for(int i=0; i<arr.length; i++) {
			if(arr[i] == ele) {
				ind = i;
			}
		}
		System.out.println(ind);
	}

}
