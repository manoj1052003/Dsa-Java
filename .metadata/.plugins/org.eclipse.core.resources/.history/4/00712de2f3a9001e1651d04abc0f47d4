package com.novdec;

public class armstong {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		int num = 153;
		int temp = num;
		int count = 0;
		int res = 0;
		
		while(num>0) {
			num=num/10;
			count+=1;
		}
		
		System.out.println(count);
		
		while (temp > 0) {
			int rem = temp % 10;
			res = int( Math.pow(rem, count))+res;
			temp = temp / 10;
		}
		
		System.out.println(res);

	}
	

}
