package com.august;
import java.util.*;
public class str1 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		String s = "pentagon";
		int count = 0;
		//System.out.println(Arrays.toString(arr));
		for(int i=0; i<s.length(); i++) {
			char ch =  s.charAt(i);
			if(ch == 'a' || ch == 'e' || ch == 'i' || ch == 'o' || ch == 'u') {
				count++;
			}
		}
		System.out.println(count);
	}

}
