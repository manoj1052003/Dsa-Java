package com.july24;

public class array3 {
	
	public static int l_search(int [] arr, int key) {
		for(int i=0; i<arr.length; i++) {
			if(arr[i] == key) {
				return i;
			}
		}
		return -1;
	}

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int [] arr = {12,23,78,56,34,90,14,38};
		int key = 78;
		System.out.println(l_search);
		
	}

}
