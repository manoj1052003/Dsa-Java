package com.april;

public class hcf {
	
	public static int find_hcf(int a, int b) {
		int smallest = 0;
		int cf = 0;
		smallest = (a<b)?a:b;
		for (int i=smallest; i>=1; i--) {
			if(a%i==0 && b%i==0) {
				cf = i;
				break;
			}
		}
		return cf;
	}

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int a = 10;
		int b = 20;
		int res = find_hcf(a,b);
		System.out.println("hcf of "+a+" and "+b+" is "+ res);
		

	}

}
