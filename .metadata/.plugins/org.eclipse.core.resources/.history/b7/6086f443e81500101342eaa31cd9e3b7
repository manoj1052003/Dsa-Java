package com.june19;

public class patterncho {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int rows = 5;
		for(int i=1; i<=rows; i++) {
			for(int j=1; j<=rows; j++) {
				if(i==1) {
					System.out.print("*"+" ");
				}
				else {
					System.out.println(" "+" ");
				}
			}
			System.out.println();
		}
		
	}

}
