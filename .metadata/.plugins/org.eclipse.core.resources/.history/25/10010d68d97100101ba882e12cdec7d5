package com.basicop;


public class Class1assign {
	

	public static void main(String[] args) {
		int [] arr = {-7,10,-8,9,-3,-2,6,-6};
		
		for(int i=0; i<arr.length; i++) {
			for(int j=i; j<arr.length; j++) {
				int sum = 0;
				for(int k=i; k<=j; k++) {
					System.out.print(arr[k] + " ");
					sum+=arr[k];
				}
				System.out.println(" : sum "+ sum);
				System.out.println();
			}
		}
		
		
	}

}
