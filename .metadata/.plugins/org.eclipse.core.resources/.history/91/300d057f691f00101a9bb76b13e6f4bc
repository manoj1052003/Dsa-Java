package com.sepnov;

import java.util.Arrays;

public class num1 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int sum = 0;
		int[] arr = { 12, 23, 34, 45, 56, 67, 78 };
		for (int i = 0; i < arr.length; i++) {
			if(arr[i]%2!=0) {
				sum = sum + arr[i];
			}
			
		}
		System.out.println(sum);

	}

}
