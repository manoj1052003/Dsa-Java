package com.sepnov;

import java.util.Arrays;
import java.util.Scanner;

public class num1 {

	public static void main(String[] args) {
		
		String s = "pentagon";
		String res = "";
		for(int i=0; i<s.length(); i++) {
			char ch = s.charAt(i);
			res+=ch;
		}
		System.out.println(res);
		
		
	}

}
