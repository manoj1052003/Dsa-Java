package com.numbers;

public class hcf {
	
	static int find_hcf(int a, int b) {
		
		int hcf = 0;
		
		for (int i = 1; i<=a; i++) {
			
			if (a%i==0 && b%i==0) {
				
				hcf=i;
			}
		}
		
		return hcf;
	}

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		int a = 4;
		int b = 8;
		int res = (find_hcf(a, b));
		System.out.println(res);
		

	}

}
