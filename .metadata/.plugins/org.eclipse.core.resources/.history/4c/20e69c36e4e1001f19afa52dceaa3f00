package com.onlinejavapro;

import java.util.Arrays;

public class class28string5 {

	public static void reverse(String s) {
		String res = "";
		for(int i=0; i<s.length(); i++) {
			res = s.charAt(i) + res;
		}
		System.out.print(res);
	}
	
	
	
	
	
	
	//india
	public static void con_up(String s) {
		String res = "";
		for(int i=0; i<s.length(); i++) {
			if(s.charAt(0) >= 97 && i==0 ) {
				int nas = s.charAt(i) -32;
			}
		}
	}
	
	
	
	
	
	
	
