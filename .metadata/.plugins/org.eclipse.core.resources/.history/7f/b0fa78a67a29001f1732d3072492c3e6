package com.strings;

public class substringgg {
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		String s = "abcd";
		for (int i=0; i<s.length();i++) {
			//String res = "";
			for (int j=i; j<s.length();j++) {
				//res = res + s.charAt(j);
				//System.out.println(res);
				System.out.println(s.substring(i,j+1));
			}
		}
		
	}

}
