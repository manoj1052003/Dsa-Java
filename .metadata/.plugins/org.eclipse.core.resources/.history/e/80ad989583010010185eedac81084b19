package com.jan26;

public class class14num4 {
	
	public static int rev(int num) {
		int res = 0;
		while(num!=0) {
			int rem = num % 10;
			res = (res * 10) + rem;
			num = num / 10;
		}
		return res;
	}

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int k=1;
		for(int i=1; i<=6; i++) {
			for(int j=1; j<=i; ) {
				if(k==rev(k)) {
					System.out.print(k+" ");
					j++;
				}
				k++;
			}
			System.out.println();
		}
		
		
		
	}

}
