package com.numbers;

public class hcfeuclid {
	
	static int find_hcf(int a, int b) {
		
		while(a != b) {
			
			if (a > b) {
				
				a= a -b;
				
			}
			else {
				
				b=b-a;
			}
			
		}
		
		return a;	
	}

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		int a = 1;
		int b = 10;
		int res = (find_hcf(a, b));
		System.out.println(res);
		

	}

}
