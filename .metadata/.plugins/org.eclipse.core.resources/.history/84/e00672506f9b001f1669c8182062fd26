package com.july24;

public class rec2 {
	
	public static void print_num(int num,int sum) {
		
		if(num==11) {
			System.out.println(sum);
			return;     // num = 4
		}				// sum  = 6
		print_num(num+1,sum+num);
	}


	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int num = 1;
		int sum = 0;
		print_num(num,sum);
	}
}
