package com.opr;


public class Pattern3 {
	
	public static void main(String[] args) {
		
		int [][] arr = {{1,2,3},
						{4,5,6},
						{7,8,9}}; 
		int res1 [] = new int [arr.length];
		int res2 [] = new int [arr.length];
		int i = 0;
		for(int i=0; i<arr.length; i++) {
			for(int j=0; j<arr.length; j++) {
				if(i==j) {
					res1[i] = arr[i][j];
				}
				if(i+j==arr.length-1) {
					res2[i] = arr[i][j];
				}
			}
		}
	}

}
