package com.june;

public class num3 {
	
	public static int reverse(int num) {
		int res = 0;
		while(num!=0) {
			int rem = num % 10;
			res = (res * 10)+ rem;
			num = num / 10;
		}
		return res;
	}

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int num = 12321;
		int rev = reverse(num);
		if(num==rev) {
			System.out.println("its palindrome");
		}
		else {
			System.out.println("its not");
		}
		
	}

}
