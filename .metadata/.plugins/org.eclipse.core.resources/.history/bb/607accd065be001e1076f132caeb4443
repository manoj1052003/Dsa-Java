package com.novdec;
import java.util.Scanner;

public class usemethods {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner sc = new Scanner(System.in);
		int a = sc.nextInt();
		int b = sc.nextInt();
		int smal=0;
		int cf=1;
		if (a<b) {
			smal = a;
		}
		else {
			smal=b;
		}
		
		for (int i=1; i <=smal; i++) {
			if (a%i==0 && b%i==0) {
				cf = i;
			}
		}
		System.out.println(cf);
		
		

	}

}
