package com.jan26;

import java.util.Arrays;

public class class14num4 {
	
	public static void sub_array(int [] arr, int s) {
		int s_length = 0;
		for(int i=0; i<arr.length; i++) {
			int sum = 0;
			for(int j=i; j<arr.length; j++) {
				sum = sum + arr[j];
				if(sum>=s) {
					count++;
				}
			}
		}
	}
	
	
	public static void main(String[] args) {
		int [] arr = {2,3,1,2,4,3};
		int s = 7;
		
	}

}
