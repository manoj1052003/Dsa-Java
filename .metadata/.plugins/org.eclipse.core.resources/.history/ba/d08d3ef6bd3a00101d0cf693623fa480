package com.deque.doubleended;

class Node{
	int data;
	Node next;
	
	Node(int data){
		this.data = data;
		this.next = null;
	}
}

class deque{
	Node front;
	Node rear;
	
	public void add_first(int ele) {
		Node temp = new Node(ele);
		if(front == null) {
			front = temp;
			rear = temp;
			return;
		}
		temp.next = front;
		front = temp;
	}
	
	public void add_last(int ele) {
		Node temp = new Node(ele);
		if(rear == null) {
			front = temp;
			rear = temp;
			return;
		}
		rear.next = temp;
		rear = temp;
	}
	
	public void delete_first() {
		
	}
	
	public void delete_last() {
		
	}
}


public class first {

	public static void main(String[] args) {
		

	}

}
