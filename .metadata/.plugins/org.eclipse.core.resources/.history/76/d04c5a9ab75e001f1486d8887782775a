package com.kiran;

public class dj { 

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		String s = "aaabbccdd";//a3b2c2d2
		int i = 0;
		int j = 0;
		int count=1;
		while(i<s.length()) {
			String res = "";
			while(j<s.length()-1) {
				if(s.charAt(j)==s.charAt(j+1)) {
					res = res + s.charAt(j);
					count++;
					j++;
				}
				else {
					System.out.print(s.charAt(i)+count);
					count=0;
					i=j;
				}
			}
		}
		
		
	}

}
