package com.june19;

public class ninthclass {

	public static void main(String[] args) {
		int num = 111;
		int res = 0;
		while(num!=0) {
			int rem = num % 10;
			res = (res * 10) + rem;
			num = num / 10;
		}
		System.out.println(num);
		System.out.println(res);
		if(num == res) {
			System.out.println("its palin");
		}
		else {
			System.out.println("its not");
		}
		
	}
}
