package com.grooming;

public class kanini {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int [] arr = {12,23,-20,-20,56,6,0,0};
		int count = 0;
		for (int i = 0; i < arr.length-1; i++) {
			if(arr[i] == arr[i+1]) {
				count++; //2
			}
		}
		System.out.println(count);
	}

}
