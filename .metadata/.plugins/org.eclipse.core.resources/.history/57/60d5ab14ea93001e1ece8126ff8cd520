package com.pattern;

public class fifthcls {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int rows = 5;
		
		
		for (int i=1; i<=rows; i++) {
			
			int k = rows+1-i;
			
			for (int j=1; j<=rows-i; j++) {
				
				System.out.print(" "+" ");
			}
			
			for (int j=1; j<=i; j++) {
				
				System.out.print(k+" ");
				
				k++;
			}
			
			System.out.println();
		}
	}

}
