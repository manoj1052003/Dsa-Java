package com.oct21CSR;

public class basicclass3 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int day = 4;
		switch(day) {
		case 1:
			System.out.println("dry day");
			
		case 2:
			System.out.println("tuesday");
			
		case 3:
			System.out.println("wednesday");
			
		case 4:
			System.out.println("thursday");
			
		case 5:
			System.out.println("fry day");
			
		case 6:
			System.out.println("half day");
			
		case 7:
			System.out.println("party day");
		}
		
		
	}

}
