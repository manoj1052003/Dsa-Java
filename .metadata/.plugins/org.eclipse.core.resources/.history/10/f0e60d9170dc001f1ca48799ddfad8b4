package com.novdecnew;

public class class14 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int rows = 9;
		int cols = 5;
		for(int i=1; i<=rows; i++) {
			for(int j=1; j<=cols; j++) {
				if(j==1 && i%2==1 || i==j) {
					System.out.print("*"+" ");
				}
				else {
					System.out.print(" "+" ");
				}
			}
			System.out.println();
		}
		
	}

}
