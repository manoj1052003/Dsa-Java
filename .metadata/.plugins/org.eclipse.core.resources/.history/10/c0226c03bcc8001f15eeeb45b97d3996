package com.onlinejavapro;

public class class10 {
	
	public static void f_fact(int num) {
		int sum = 0;
		for(int i=1; i<num ; i++) {
			if(num%i==0) {
				sum = sum + i;
			}
		}
		System.out.println(sum);
	}
	

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int num = 6;
		f_fact(num);
		
	}

}
