package com.novdecnew;

public class class22 {

	public static void main(String[] args) {
		// .toUpperCase();
		String s = "PEntaGOn";
		String res = "";
		for(int i=0; i<s.length(); i++) {
			char ch = s.charAt(i);
			if(ch>=97 && ch<=122) {
				char ch1 = (char)(ch - 32);
				//System.out.println(ch1);
				res = res + ch1;
			}
			else {
				//System.out.println(ch);
				res = res + ch;
			}
		}
		System.out.println(res);
	}

}
