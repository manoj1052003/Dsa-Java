package com.numbers;

import java.util.HashMap;

public class classmeth {
	
	public static void rev(int num) {
		int res = 0;
		while(num !=0) {
			int rem= num % 10;
			res = (res * 10) + rem;
			num= num / 10;
		}
		System.out.println(res);
	}
	
	public static void rev_rec(int num, int res) {
		if(num == 0 ) {
			System.out.println(res);
			return;
		}
		rev_rec(num/10,(res * 10) + num % 10);
		
	}
	

