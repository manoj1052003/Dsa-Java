package com.june19;

public class thirteen {
	
	public static void p_fibo(int num,int a, int b,int count) {
		if(count==8) {
			return;
		}
		System.out.println(a);
		p_fibo(num,b,a+b,count+1);
	}

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int num = 8;
		p_fibo(num,0,1,0);
		
	}

}
