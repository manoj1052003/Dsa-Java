package com.june;
import java.util.*;
public class stringbasic1 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		String s = "a b";
		int countv = 0;
		int countc = 0;
		for(int i=0; i<s.length(); i++) {
			char c = s.charAt(i);
			if( c=='a' || c=='e' || c=='i' || c=='o' || c=='u' ) {
				countv++;
			}
			else {
				countc++;
			}
		}
		System.out.println(countv);
		System.out.println(countc);
		
		
	}

}
