package com.basic;

public class Logicalopr {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int a = 10;
		a = a + 2;
		System.out.println(a);
		a = a + 2;
		System.out.println(a);
		a += 2;
		System.out.println(a);
		a -= 10;
		System.out.println(a);
		a *= 10;
		System.out.println(a);
		a /= 6;
		System.out.println(a);
		a %= 2;
		System.out.println(a);
		
		
	}

}
