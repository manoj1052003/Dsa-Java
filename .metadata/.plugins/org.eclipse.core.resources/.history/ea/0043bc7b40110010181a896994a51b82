package com.patterns;

public class class4 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		String s = "hello";
		String res = "";
		for(int i=0; i<s.length(); i++) {
			
			for(int j=i; j<s.length(); j++) {
				res = res + s.charAt(j);
				System.out.println(res);
			}
		}
	}

}
