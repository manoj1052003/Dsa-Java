package com.july24;

public class rec2 {
	
	public static void print_sum(int num,int sum) {
		if(num==1) {
			System.out.println(num);
			return;
		}
		print_sum(num-1,sum);
		System.out.println(num);
		
	}

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int num = 2;
		print_sum(num,0);
	}

}
