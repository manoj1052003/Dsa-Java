package com.sepnov;

public class num1 {
	
	public static int l_search(int [] arr, int key) {
		for(int i=0; i<arr.length; i++) {
			if(arr[i] == key) {
				return i;
			}
			else {
				return -1;
			}
		}
		return -1;
	}
	
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int [] arr = { 12, 23, 34, 45, 56, 67 };
		int key = 34;
		System.out.println(l_search(arr, key));
		
		
		
		
		

	}

}
