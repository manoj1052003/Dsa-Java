package com.june19;

public class seventhclass {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int rows = 5;
		for(int i=1; i<=rows; i++) {
			int k = i;
			for(int j=1; j<=rows; j++) {
				if(i==1||j==1||i==rows||j==rows) {
					System.out.print((char)(i+64)+" ");
					i++;
				}
				else {
					System.out.print(" "+" ");
				}
			}
			System.out.println();
		}
	}

}
