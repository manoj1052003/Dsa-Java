package com.graph;

class AdjMat{
	
	int [][] arr;
	
	AdjMat(int Nodes) {
		arr = new int[Nodes][Nodes];
	}
	
	public void con_nodes(int src, int des) {
		arr[src][des] = 1;
		arr[des][src] = 1;
	}
	
	public void print() {
		for(int i=0; i<arr.length; i++) {
			for(int j=0; j<arr.length; j++) {
				System.out.print(arr[i][j]+" ");
			}
			System.out.println();
		}
	}
}

public class First {

	public static void main(String[] args) {
		AdjMat am = new AdjMat(6);
		am.con_nodes(0, 1);
		am.con_nodes(0, 2);
		am.con_nodes(1, 3);
		am.con_nodes(1, 4);
		am.con_nodes(2, 4);
		am.con_nodes(3, 5);
		am.con_nodes(4, 5);
		am.print();
		

	}

}
