package com.grooom;

public class cog3 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		String s = "a2b4e4f2g3";
		String res = "";
		for(int i=0; i<s.length(); i++) {
			char curr = s.charAt(i);
			int next = (int) s.charAt(i+1);
			for(int j=1; j<=next; j++) {
				res = res + curr;
			}
			i++;
		}
		System.out.println(res);
	}

}
