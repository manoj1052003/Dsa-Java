package com.sepnov;
import java.util.*;
public class class17 {
	
	public static void l_search(int [] arr, int key) {
		boolean ex = false;
		for(int i=arr.length-1; i>=0; i--) {
			if(arr[i]==key) {
				System.out.println(i);
				ex = true;
			}
		}
		if(ex == false) {
			System.out.println(-1);
		}
	}
	
	
	
	
	public static int b_search(int [] arr, int key) {
		int low = 0;
		int high = arr.length-1;
		while(low<=high) {
			int mid = (low+high)/2;
			if(arr[mid] == key) {
				return mid;
			}
			else if(key>arr[mid]) {
				low = mid+1;
			}
			else {
				high = mid-1;
			}
		}
		return -1;
	}
	
	
	
	public static void r_anti(int [] arr) {
		int temp = arr[0];
		for(int i=1; i<arr.length; i++) {
			arr[i-1] = arr[i];
		}
		arr[arr.length-1] = temp;
		
	}

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int [] arr = {12,23,34,45,56,67,78};
		for(int i=1; i<=4500; i++) {
			r_anti(arr);
		}
		System.out.println(Arrays.toString(arr));
	}

}
