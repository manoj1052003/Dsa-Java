package com.basics;

public class Class2 {

	public static void main(String[] args) {
		String s = "pentagon";
		int v_count = 0;
		int c_count = 0;
		for(int i=0; i<s.length(); i++) {
			char ch = s.charAt(i);
			if(ch == 'a' || ch == 'e' || ch == 'i' || ch == 'o' || ch == 'u') {
				v_count++;
			}
			else {
				c_count++;
			}
		}
		System.out.println(v_count);
		System.out.println(c_count);
	}
}
