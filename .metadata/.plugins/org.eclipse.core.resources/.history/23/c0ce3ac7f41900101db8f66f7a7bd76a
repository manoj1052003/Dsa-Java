package com.kiran;

public class today {
	
	public static int c_digit(int num) {
		int count = 0;
		while(num!=0) {
			num = num / 10;
			count++;
		}
		return count;
	}
	
	public static int arm(int num) {
		int res = 0;
		int d = c_digit(num);
		while(num!=0) {
			int rem = num% 10;
			res = (int)Math.pow(rem, d) + rem;
			num = num/10;
		}
		return res;
	}

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int num = 153;
		int ans = arm(num);
		if(num==ans) {
			System.out.println("its armstrong");
		}
		else {
			System.out.println("its not");
		}
	}

}
