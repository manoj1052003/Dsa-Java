package com.grooming;

import java.util.Scanner;

public class kanini {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		String s = "pentagon";
		String res = "";
		for(int i=0; i<s.length(); i++) {
			res = res + s.charAt(i);
		}
		
		System.out.println(res);
		
	}

}
