package com.april;

public class armstrong {
	
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int rows = 5;
		int sum = 0;
		for(int i=1; i<=rows; i++) {
			sum+=i;
		}
		for(int i=1; i<=rows; i++) {
			for(int j=1; j<=i; j++) {
				System.out.println(sum+" ");
				sum--;
			}
		}
	
	}

}
