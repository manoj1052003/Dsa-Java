package com.kiran;

public class function {

	static String reverse_string(String s) {
		String t="";
		int n = s.length()-1;
		for (int i = n; i>=0 ; i-- ) {
			Char data = s.charAt(i);
			t=t+data;
		}
		return t;
		
	}
	
	public static void main(String[] args) {
		String s="pentagon";
		System.out.println(reverse_string(s));
					
	}
}
