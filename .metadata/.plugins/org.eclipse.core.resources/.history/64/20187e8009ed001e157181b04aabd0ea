package com.csr;

public class rec {
	public static int print_num(int num) {
		if(num==1) {
			return 1;
		}
		return print_num(num-1)*num;
			
	}

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int num=5;
		int ans = print_num(num);
		System.out.println(ans);
		
		
	}

}
