package com.pattern;

public class todaypattern {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int rows = 5;
		int k = 65;
		for (int i=1; i<=rows; i++) {
			for (int j=1; j<=i; j++) {
				System.out.print((char)k);
				k++;
			}
			System.out.println();
		}

	}

}
