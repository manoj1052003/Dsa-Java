package com.sepnov;

public class class15 {
	
	public static void euclid(int a, int b) {
		while(a!=b) {
			if(a>b) {
				a = a - b;
			}
			else {
				b = b - a;
			}
		}
		System.out.println(a);
	}
	
	
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int a = 25;
		int b = 15;
		euclid(a,b);
		
		
	}
}