package com.basicop;

public class Class1assign {
	
	public static int sqr_sum_digits(int num) {
		int sum = 0;            // num = 0
		while(num!=0) {         // sum = 61
			int rem = num % 10;  // rem = 6
			sum = sum + (rem * rem); //
			num = num / 10;
		}
		return sum;
	}

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int num = 18;
		int res = sqr_sum_digits(num);
		while(res!=1 && res!=4) {
			res = sqr_sum_digits(res);
		}
		if(res == 1) {
			System.out.println("its happy");
		}
		if(res == 4) {
			System.out.println("its not");
		}
		
	}

}
