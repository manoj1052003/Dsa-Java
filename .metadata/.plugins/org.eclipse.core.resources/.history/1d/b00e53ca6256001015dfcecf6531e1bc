package com.basics;

public class Class2 {
	
	public static void main(String[] args) {
		  int [] arr = {82,42,32,92,2,12,27};
		  for(int i=0; i<arr.length; i++) {
			  for(int j=0; j<arr.length-1; j++) {
				  if(arr[j]>arr[j+1]) {
					  int temp = arr[j];
					  arr[j] = arr[j+1];
					  arr[j+1] = temp
				  }
			  }
		  }

	}

}
