package com.jan26;

import java.util.Arrays;

import com.csr.arr1;

public class class14num4 {
	
	public static boolean check_sorted(int [] arr) {
		for(int i=0; i<arr.length-1; i++) {
			if(arr[i] > arr[i+1]) {
				return false;
			}
		}
		return true;
	}
	
	public static void main(String[] args) {
		int [] arr1 = {12,23,34,45,56,67,78};
		int [] arr2 = {12,23,34,45,56,67,78};
		System.out.println(arr1==arr2);
		
	}

}
