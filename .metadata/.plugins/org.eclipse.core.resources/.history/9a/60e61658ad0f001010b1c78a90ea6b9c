package com.basicspro;

import java.util.Arrays;

public class firdtclass {
	
	public static void main(String[] args) {
		String s = "hello";
		for(int i=0; i<s.length(); i++) {
			char ch = s.charAt(i);
			if(ch >=97 && ch<=122) {
				System.out.println((char)(ch-32));
			}
			else {
				System.out.println(ch);
			}
		}
	}
}
	
