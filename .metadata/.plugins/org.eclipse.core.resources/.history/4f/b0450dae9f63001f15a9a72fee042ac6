package com.june;

public class str1 {
	
	public static String reverse(String s) {
		String res = "";
		for(int i=0; i<s.length();i++) {
			res = s.charAt(i) + res ;
		}
		return res;
	}

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		String s = "birthday";
		String res = reverse(s);
		System.out.println(res);
		
		
		
	}

}
