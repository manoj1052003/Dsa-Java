package com.june;
import java.util.*;
public class pattern11 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int rows = 5;
		int k = 1;
		for(int i=1; i<=rows; i++) {
			for(int j=1; j<=i; j++) {
				System.out.print((i+j+1)%2+" ");
				k++;
			}
			System.out.println();
		}
	}
}
