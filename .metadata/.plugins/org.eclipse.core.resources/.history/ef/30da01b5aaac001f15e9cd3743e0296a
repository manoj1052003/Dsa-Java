package com.sepnov;

public class class2 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		for(int i = 1; i<=5 ; i+=1) { 
			System.out.println("hi");
		}
		
		int k =1;     // k = 6
		while(k<=5) {
			System.out.println("hello");
			k++;
		}
		System.out.println("end");
		
	}

}
