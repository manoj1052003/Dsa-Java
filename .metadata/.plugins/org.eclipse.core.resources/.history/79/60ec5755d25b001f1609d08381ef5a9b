package com.kiran;

public class nearestpalin {
	
	public static boolean find_ans(int num) {
		int res = 0;
		while(num!=0) {
			int rem = num % 10;
			res = (res * 10) + rem;
			num/=10;
		}
		if(num==res) {
			return true;
		}
		else {
			return false;
		}
	}

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int num = 125;
		boolean res = find_ans(num);
		while(res==false) {
			find_ans(num+1);
		}
		System.out.println(num);
	}

}
