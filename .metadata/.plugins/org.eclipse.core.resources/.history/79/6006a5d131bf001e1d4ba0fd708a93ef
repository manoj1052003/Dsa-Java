package com.novdec.number;

public class reversenumb {
	
	public static int reverse(int num) {
		int res = 0;
		while(num>0) {
			int rem = num % 10;
			res = (res*10)+rem;
			num = num / 10;
		}
		return res;
		System.out.println(num);
	}

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int num = 123;
		int res = reverse(num);
		//System.out.println(res);
		//System.out.println(num);
		

	}

}
