package com.sepnov;

public class num1 {
	
	public static void find_ans(String s) {
		String res = "";
		for(int i=0; i<s.length(); i+=2) {
			int n = s.charAt(i+1);
			System.out.println(n);
		}
	}
	
	
	
	public static void main(String[] args) {
		String s = "a2b5c3";
		//output : aabbbbbccc
		find_ans(s);

	}
 
}
