package com.jan26;

import java.util.Scanner;

public class class11num1 {
	
	

	public static void main(String[] args) {
		int [] arr = {-4,9,-2,1,-5};
		int sum = 0;
		int max_sum = 0;
		for(int i=0; i<arr.length; i++) {
			sum = 0;
			for(int j=i; j<arr.length; j++) {
				sum = sum + arr[j];
				if(sum>max_sum) {
					max_sum = sum;
				}
			}
		}
		System.out.println(max_sum);
		
		
	
	}
}
