package com.kiran;

public class nearestpalin {
	
	public static int find_ans(int num) {
		int res = 0;
		while(num!=0) {
			int rem = num % 10;
			res = (res * 10) + rem;
			num/=10;
		}
		return num;
	}

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int num = 125;
		for(int k=1; ;k++) {
			int res = find_ans(k);
			if(res==k) {
				System.out.println(k);
			}
		}
		
		
	}

}
