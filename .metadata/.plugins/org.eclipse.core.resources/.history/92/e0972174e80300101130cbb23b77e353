package com.jan26;

public class class14num4 {
	
	public static int find_ans(int num) {  // num = 0
		int res = 0;                        //res = 65
		while(num != 0) {
			int rem = num % 10;        //rem = 1
			res = (rem * rem)+res;
			num = num / 10;
		}
		return res;
	}
	
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		for(int i=1; i<=10000; i++) {
			int num = i;
			int res = find_ans(num);
			while(res!=1 && res!=4) {
				res = find_ans(res);
			}
			if(res==1) {
				System.out.println(num);
			}
		}
		
		
		
	}

}
