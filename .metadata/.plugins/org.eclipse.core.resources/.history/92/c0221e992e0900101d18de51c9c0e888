package com.jan26;

import java.util.Arrays;

public class class14num4 {
	
	public static void main(String[] args) {
		int [] arr = {2,4,6,8,10,12,14,16};
		int left = 0;
		int right = arr.length-1;
		while(left <= right) {
			int temp = arr[left];
			arr[left] = arr[right];
			arr[right] = temp;
			left++;
			right--;
		}
		
	}

}
