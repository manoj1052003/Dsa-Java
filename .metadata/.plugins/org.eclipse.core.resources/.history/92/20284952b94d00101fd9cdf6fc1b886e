package com.numbers;


public class classmeth { 
	
	public static int numsum(int num) {
		int sum = 0;
		while(num != 0) {
			int rem = num % 10;
			sum = sum + rem;
			num = num / 10;
		}
		return sum;
	}
	
	
	public static void main(String[] args) {
		int num = 99;
		int res = num+1;
		while(numsum(num)*2 != numsum(res)) {
			res++;
		}
		System.out.println(res);
	}
}
