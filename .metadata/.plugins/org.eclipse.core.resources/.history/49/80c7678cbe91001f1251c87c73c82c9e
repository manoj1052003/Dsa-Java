package workshop;

public class day4 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int num = 121;
		int res = 0;
		while(num!=0) {
			int rem = num % 10;
			res = (res * 10)+rem;
			num = num / 10;
		}
		if(num==res) {
			System.out.println("its palindrome");
		}
		else {
			System.out.println("its not");
		}
		
	}

}
