package com.sepnov;

public class class17 {
	
	public static void l_search(int [] arr, int key) {
		boolean ex = false;
		for(int i=arr.length-1; i>=0; i--) {
			if(arr[i]==key) {
				System.out.println(i);
				ex = true;
			}
		}
		if(ex == false) {
			System.out.println(-1);
		}
		
	}

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		int [] arr = {12,23,34,45,56,67,45,78};
		int key = 455;
		l_search(arr, key);
		
	}

}
