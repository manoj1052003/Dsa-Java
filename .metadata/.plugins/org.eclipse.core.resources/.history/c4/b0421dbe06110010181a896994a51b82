package com.patterns;

import java.util.Arrays;

public class class3 {
	
	public static String rev(String s) {
		String res = "";
		for(int i=s.length()-1; i>=0; i--) {
			res = res + (char)(s.charAt(i)+1);
		}
		return res;
	}
	

	public static void main(String[] args) {
		
		String s = "ABDEF";
		String res = rev(s);
		System.out.println(res);
		
		
	}

}

