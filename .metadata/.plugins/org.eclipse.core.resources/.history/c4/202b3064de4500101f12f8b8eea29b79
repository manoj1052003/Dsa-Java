package com.opr;

import java.util.Arrays;

public class Pattern3 {
	
	public static void main(String[] args) {
		for(int i=0; i<=5; i++) {
			for(int j=0; j<=6; j++) {
				if((i==1 && j%3==0) || (i==0 && j%3!=0)) {
					System.out.print("*"+" ");
				}
				else {
					System.out.print(" "+" ");
				}
			}
			System.out.println();
		}
	}

}
