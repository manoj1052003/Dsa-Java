package com.novdecnew;

public class class14 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int rows = 5;
		int k =0;
		for(int i=1; i<=rows; i++) {
			for(int j=1; j<=rows-i; j++) {
				System.out.print(" "+" ");
			}
			for(int j=1; j<=i; j++) {
				System.out.print((char)((k%4)+65)+" ");
				k++;
			}
			System.out.println();
		}
	}

}
