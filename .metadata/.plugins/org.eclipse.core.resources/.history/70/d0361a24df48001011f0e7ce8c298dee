package com.numbers;

public class classmeth {
	public static String reverse(String s) {
		String res = "";
		for(int i=s.length()-1;i>=0;i--) {
			res = res + s.charAt(i);
		}
		return res;
	}
	
	public static void main(String[] args) {
		String s = "hello"; //olleh
		String res = reverse(s);
		System.out.println(res);
		
			
	}
}
