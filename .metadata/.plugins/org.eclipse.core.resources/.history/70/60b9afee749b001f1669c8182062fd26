package com.july24;

public class rec2 {
	
	public static int print_num(int num,int res) {
		if(num==0) {
			return res;
		}
		return print_num(num/10,(res*10)+(num%10));
		
	}

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int num = 123;
		int res = 0;
		int res1 = print_num(num,res);
		System.out.println(res1);
	}
}
