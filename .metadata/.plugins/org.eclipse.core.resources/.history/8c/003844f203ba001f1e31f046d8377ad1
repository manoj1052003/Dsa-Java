package com.sepnov;

public class class15 {
	
	public static void p_fact(int num, int i) {
		if(i>num) {
			return;
		}
		
		p_fact(num,i+=1);
		
		if(num%i==0) {
			System.out.println(i);
		}
	}
	
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int num = 24;
		int i = 1;
		p_fact(num,i);
		
		
	}
}