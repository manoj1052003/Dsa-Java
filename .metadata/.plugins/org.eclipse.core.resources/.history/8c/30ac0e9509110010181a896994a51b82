package com.patterns;

public class class3 {

	public static void main(String[] args) {
		String s = "malayalam";
		for(int i=0; i<s.length(); i++) {
			String res = "";
			for(int j=i; j<s.length(); j++) {
				res = res + s.charAt(j);
				System.out.println(res);
				//System.out.println(s.substring(i, j+1));
			}
		}
		
	}

}

