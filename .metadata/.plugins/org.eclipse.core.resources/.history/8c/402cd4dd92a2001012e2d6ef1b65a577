package com.graph;

import java.util.Arrays;

class AdjList{
	int Nodes;
	int [][] arr;
	
	AdjMat(int Nodes) {
		this.Nodes = Nodes;
		arr = new int [Nodes][Nodes];
		//System.out.println(Arrays.deepToString(arr));
	}
	public void AddEdge(int src, int des) {
		arr[src][des] = 1;
		arr[des][src] = 1;
	}
	public void print_mat() {
		for(int i=0; i<Nodes; i++) {
			for(int j=0; j<Nodes; j++) {
				System.out.print(arr[i][j]+" ");
			}
			System.out.println();
		}
	}
}

public class First {

	public static void main(String[] args) {
		AdjList am = new AdjList(6);
		am.AddEdge(0, 1);
		am.AddEdge(0, 2);
		am.AddEdge(1, 2);
		am.AddEdge(1, 3);
		am.AddEdge(1, 4);
		am.AddEdge(2, 5);
		am.AddEdge(4, 5);
		am.print_mat();

	}

}
