package com.june19;
import java.util.*;
public class arr1 {
	
	public static void find_max (int [] arr) {
		int max = 0;
		for(int i=0; i<arr.length; i++) {
			if(arr[i]>max) {
				max= arr[i];
			}
		}
		System.out.println(max);
		
	}
	

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int [] arr = {10,18,7,29,32,2,30};
		find_max(arr);
		
		
		
		
		
	}

}
