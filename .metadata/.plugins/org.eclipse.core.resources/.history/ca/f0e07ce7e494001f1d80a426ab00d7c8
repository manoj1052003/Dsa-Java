package workshop;
import java.util.*;
public class day8 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		String s = "pentagon space";
		int count_v = 0;
		int count_c = 0;
		for(int i=0; i<s.length(); i++) {
			char c = s.charAt(i);
			if(c=='a'|| c=='e'|| c=='i'|| c=='o'|| c=='u') {
				//System.out.println(c);
				count_v++;
			}
			else {
				count_c++;
			}
		}
		System.out.println("no of vowels are : "+count_v);
		System.out.println("no of conso are : "+count_c);

	}

}
