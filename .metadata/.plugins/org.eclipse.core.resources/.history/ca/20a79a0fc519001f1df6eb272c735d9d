package com.pattern;

public class aprilcsrfri {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int rows = 5;
		int k =97;
		int count = 1;
		for (int i=1; i<=rows; i++) {
			for (int j=1; j<=i; j++) {
				if(count%2==0) {
					System.out.print((char)(k-32)+" ");
					k++;
					count++;
				}
				else {
					System.out.print((char)(k)+" ");
					k++;
					count++;
				}
				
					
			}
			System.out.println();
		}
	}

}
