package com.numbers;

public class primenum {
	
	static boolean check_prime(int num) {
		for (int i = 2; i < num; i++) {
			if (num %i ==0) {
				return false;
			}
		}
		return true;
	}

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		for (num=2; num<=1000; num++) {
			boolean res = check_prime(num);
			System.out.println(res);
		
		}
	}

}
