package com.csr;

public class heart {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int rows=5;
		int cols=9;
		
		for (int i=0; i<rows;i++) {
			for (int j=0;j<cols;j++) {
				if((i+j==4)||(j-i==4)||(i==4 && j%2==0)) {
					System.out.print("*"+" ");
				}
				else {
					System.out.print(" "+" ");
				}
			}
			System.out.println();
		}
	}

}
