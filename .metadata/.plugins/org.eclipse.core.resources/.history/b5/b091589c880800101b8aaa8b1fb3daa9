package com.jan26;

import java.util.Arrays;

public class class14num4 {

	
	public static void main(String[] args) {
		String s1 = "silent";
		String s2 = "listen";
		
		char [] arr1 = s1.toCharArray();
		char [] arr2 = s2.toCharArray();
		
		Arrays.sort(arr1);
		Arrays.sort(arr2);
		
		if(arr1.equals(arr2)) {
			System.out.println("its anagram");
		}
		else {
			System.out.println("its not");
		}
		
	}

}
