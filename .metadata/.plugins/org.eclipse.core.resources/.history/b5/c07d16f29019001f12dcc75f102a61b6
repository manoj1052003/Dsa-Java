package com.strings;

public class substringgg {
	
	

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		String s = "abcd";
		for (int i=0; i<s.length();i++) {
			String res = "";
			for (int j=0; j<s.length();j++) {
				res = res + s.charAt(i);
				System.out.println(res);
			}
		}
		
	}

}
