		String s = "pentagonspacen";
		HashMap<Character, Integer> map = new HashMap<>();
		//System.out.println(map);
		for(int i=0; i<s.length(); i++) {
			char ch = s.charAt(i);
			if(map.containsKey(ch)) {
				map.put(ch, (map.get(ch)+1));
				
			}
			else {
				map.put(ch, 1);
			}
		}
		System.out.println(map);
		for(char ch : map.keySet()) {
			if(map.get(ch)>1) {
				System.out.println(ch);
			}
			
		}
	}
}
