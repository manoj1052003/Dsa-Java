package com.june19;

public class arr1 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int [] arr = new int [5];
		for(int i=0; i<arr.length;i++) {
			System.out.println(arr[i]);
		}
		int k = 12;
		System.out.println("before adding");
		for(int i=0; i<arr.length; i++) {
			arr[i] = k;
			k+=10;
		}
		for(int i=0; i<arr.length;i++) {
			System.out.println(arr[i]);
		}
	}

}
