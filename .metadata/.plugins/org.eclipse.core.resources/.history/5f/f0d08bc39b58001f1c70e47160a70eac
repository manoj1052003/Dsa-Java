package com.june;
import java.util.*;
public class array1 {
	
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		int[] arr = new int[5];
		
		System.out.println(Arrays.toString(arr));
		
		
		for(int i=0; i<arr.length; i++) {
			System.out.println(arr[i]);
		}
		
	}
}
