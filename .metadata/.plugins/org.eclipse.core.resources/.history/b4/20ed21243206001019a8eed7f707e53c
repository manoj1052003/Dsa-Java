package com.jan26;

import java.util.Arrays;

public class class14num4 {

	public static void main(String[] args) {
		
		String s = "penTAgOn";
		String res = "";
		for(int i=0; i<s.length(); i++) {
			char ch = s.charAt(i);
			if(ch>=65 && ch<=90) {
				res = res + ch;
			}
			else {
				char nch = (char)(ch -32);
				res = res + nch;
			}
		}
		System.out.println(res);
		
	}

}
