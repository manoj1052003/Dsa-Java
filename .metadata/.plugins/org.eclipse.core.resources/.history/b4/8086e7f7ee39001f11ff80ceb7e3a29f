package com.june;

public class num6 {
	
	public static int check_happy(int num) {
		int sum = 0;
		while(num!=0) {
			int rem = num % 10;
			sum = rem + sum;
			num = num / 10;
		}
		return sum;
	}

	public static void main(String[] args) {
		// TODO Auto-generated method stub
			int num1 = 99;
			int num2= num1+1;
			int res1 = check_happy(num1);
			int res2 = check_happy(num2);
			
			while(res1*2 != res2) {
				num2++;
				res2 = check_happy(num2);
			}
			System.out.println(num2);
			
	}

}
