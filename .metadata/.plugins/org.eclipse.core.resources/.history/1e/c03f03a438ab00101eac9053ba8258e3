package com.timec;

public class first {
	
	public static boolean count_fact(int num) {
		int count = 0;
		for(int i=1; i<=num; i++) {
			if(num%i==0) {
				return false;
			}
		}
		return true;;
	}

	public static void main(String[] args) {
		int num = 24;
		System.out.println(count_fact(num));
		

	}

}
