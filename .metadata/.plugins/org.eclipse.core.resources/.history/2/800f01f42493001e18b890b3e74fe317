package com.pattern;

public class assignmttwo {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		int rows = 5;
		
		for (int i = 1; i<=rows; i++) {
			
			for (int j= 0; j<rows+1-i; j++) {
				
				System.out.print(j+" ");
			}
			
			System.out.println();
		}
	}

}
