package com.kiran;

public class today {
	
	public static int sum_fact(int num) {
		int sum = 0;
		for(int i=1; i<=num-1; i++) {
			if(num%i==0) {
				//System.out.println(i);
				sum=sum+i;
			}
		} 
		return sum; 
	}
	
	public static void main(String[] args) {
		
		for(int i=1; i<=10000; i++) {
			int num = i;
			int ans = sum_fact(num);
			if(num==ans) {
				System.out.println(i);
			}
		}
		
		
		
	}

}
