package com.sepoct;

public class num2 {
	
	public static int reverse(int num) {
		int res = 0;
		while(num!=0) {
			int rem = num % 10;
			res = (res * 10) + rem;
			num = num / 10;
		}
		
		return res;
	}

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int num = 121;
		int rev = reverse(num);
		System.out.println(num);
		if(rev==num) {
			System.out.println("pali");
		}
		else {
			System.out.println("not");
		}
	}

}
