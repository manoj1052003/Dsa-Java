package com.novdec;

public class firstclass {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int a = 50;
		int b = 50;
		if (a < b) {
			System.out.println(a+" is smaller");
		}
		else if (b < a){
			System.out.println(b+" is smaller");
		}
		
		else {
			System.out.println("both are equals");
			
		}
		
		
	}

}
