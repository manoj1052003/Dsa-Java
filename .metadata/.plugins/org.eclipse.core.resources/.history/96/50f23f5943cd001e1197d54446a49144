package com.novdec.array;
import java.util.Arrays;
public class twodarray {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int [] [] arr = new int [4] [4];
		System.out.println(Arrays.deepToString(arr));
	
	int value = 1;
	for (int i=0; i<4;i++) {
		for (int j=0; j<4; j++) {
			arr[i][j]=value;
			value++;
		}
	}
	System.out.println(Arrays.deepToString(arr));

}
}
