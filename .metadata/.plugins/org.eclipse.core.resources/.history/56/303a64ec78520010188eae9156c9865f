package com.numbers;

import java.util.Arrays;

public class classmeth {
	
	public static void main(String[] args) {
		int [] arr = {12,23,34,45,56,67,78,89};
		int i = 0;
		int j = arr.length-1;
		while(i < j) {
			int temp = arr[i];
			arr[i] = arr[j];
			arr[j] = temp;
		}
		System.out.println(Arrays.toString(arr));
		
	}
}
