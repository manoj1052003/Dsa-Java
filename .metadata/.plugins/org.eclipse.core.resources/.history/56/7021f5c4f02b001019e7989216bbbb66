package com.practice;

class Node{
	int data;
	Node next;
	
	Node(int data){
		this.data = data;
		this.next = null;
	}
}

class LL1 {
	
	public static void print_LL(Node head) {
		Node temp = head;
		if(head == null) {
			System.out.println("ll is empty");
		}
		else {
			while(temp!=null) {
				System.out.println(temp.data);
				temp = temp.next;
			}
		}
	}
}



public class prac {
	

	public static void main(String[] args) {
		
		Node p1 = new Node(10);
		Node p2 = new Node(20);
		Node p3 = new Node(30);
		Node p4 = new Node(40);
		
		p1.next = p2;
		p2.next = p3;
		p3.next = p4;
		
		Node head = p1;
		LL1.print_LL(head);
		
		

	}

}
