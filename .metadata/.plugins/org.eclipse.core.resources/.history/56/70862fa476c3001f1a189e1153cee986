package com.sepnov;
import java.util.*;
public class class21 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		String s = "malayalam";
		String res = "";
		for(int i=s.length()-1; i>=0; i--){
			res = res + s.charAt(i);
		}
		System.out.println(s);
		System.out.println(res);
		System.out.println(res.equals(s));
		
		
	}

}
