package com.jan26;

import java.util.Arrays;

public class class14num4 {
	
	public static String con_upp(String s) {
		String res = "";
		for(int i=0; i<s.length(); i++) {
			if(i==0 || i==s.length()-1) {
				char nch = (char)(s.charAt(i)-32);
				res = res + nch;
			}
			else {
				res = res + s.charAt(i);
			}
		}
		return res;
	}
	
	public static void main(String[] args) {
			int [] arr = {20,30,40,32,17,21};
			int key = 32;
			int ind = l_seach(arr,key);
			System.out.println(ind);
		
	}

}
