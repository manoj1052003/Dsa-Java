package com.basic;

public class Logicalopr {
	
	public static String toUpper(String s) {
		String res = "";
		for(int i=0; i<s.length();i++) {
			char ch = s.charAt(i);
			if(ch >= 97 && ch <= 122) {
				char nch = (char)(ch - 32);
				res += (nch);
			}
			else {
				res += (ch);
			}
		}
		System.out.println(res);
	}

	public static void main(String[] args) {
	
		String s = "pENtagON";
		String res = toUpper(s);
		System.out.println(res);
	}

}
