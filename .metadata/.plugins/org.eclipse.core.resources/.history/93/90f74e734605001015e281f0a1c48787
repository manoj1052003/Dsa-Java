package com.jan26;

public class class14num4 {
	
	public static int rec_factorial(int num, int res) {
		if(num==1) {
			return res;
		}
		return rec_factorial(num-1,(res*num));
	}

	public static void main(String[] args) {
		int num = 8;
		int a = 0;
		int b = 1;
		int count = 0;
		while(count<num) {
			System.out.print(a+" ");
			int res = a + b;
			a = b;
			b = res;
			count++;
		}
		
		
	}

}
