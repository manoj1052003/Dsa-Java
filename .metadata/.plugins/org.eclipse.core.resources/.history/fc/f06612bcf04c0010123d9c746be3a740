package com.numbers;


public class classmeth { 
	
	public static void sum_sqr_digit(int num) {
		int res = 0;
		while(num!=0) {
			int rem = num % 10;
			res = (rem * rem) + res;
			num = num / 10;
		}
		System.out.println(res);
	}
	
	public static void main(String[] args) {
		int num = 18;
		sum_sqr_digit(num);
		
	}
}
