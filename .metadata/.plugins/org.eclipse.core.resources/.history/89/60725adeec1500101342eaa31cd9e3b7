package com.june19;

public class patterncho {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		//int rows = 9 ;
		for(int i=0; i<=5; i++) {
			for(int j=1; j<=6; j++) {
				if((i==0 && j%3==0)) {
					System.out.print("*"+" ");
				}
				else {
					System.out.print(" "+" ");
				}
			}
			System.out.println();
		}
		
		
	}

}
