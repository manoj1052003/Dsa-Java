package com.jan26;

import java.util.Arrays;

public class class14num4 {
	
	public static void rev(int [] arr) {
		int low = 0;
		int high = arr.length-1;
		while(low<=high) {
			int temp = arr[low];
			arr[low] = arr[high];
			arr[high] = temp;
			low++;
			high--;
		}
		System.out.println(Arrays.toString(arr));
	}
	
	public static void main(String[] args) {
		
		int arr [] = {2,4,6,8,10,12};
		rev(arr);
		
	}

}
