package com.strings;
import java.util.*;

public class first {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner sc = new Scanner(System.in);
		System.out.println("enter string");
		String s = sc.nextLine();
		for (int i=0; i<s.length(); i++) {
			char c = s.charAt(i);
			System.out.println(c);
		}
		
	}

}
