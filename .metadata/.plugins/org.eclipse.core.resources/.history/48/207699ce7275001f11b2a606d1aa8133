package com.june19;

public class arr1 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int [] arr = new int[6];
		for(int i=0;i<arr.length;i++) {
			System.out.println(arr[i]);
		}
		System.out.println("after adding");
		//add values to array
		int k =100;
		for(int i=0; i<arr.length; i++) {
			arr[i] = k;
			k+=100;
		}
		for(int i=0;i<arr.length;i++) {
			System.out.println(arr[i]);
		}
		
		
		
		
		
	}

}
