package com.opr;


class arr{
	int [] arr = new int [10];
}



public class ArrayListKiran {
	
	int [] arr;
	int capacity;
	int size;
	
	public void add(int ele) {
		if(size == capacity) {
			grow();
		}
		arr[size++] = ele;
	}
	
	public void grow() {
		capacity *= 2;
		int [] newarr = new int [capacity];
	}

	public static void main(String[] args) {
		

	}

}
