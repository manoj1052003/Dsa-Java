package com.jan26;

public class class4patt2 {

	public static void main(String[] args) {
		
		int num = 121;
		int temp = num;
		int res = 0;
		while(num != 0) {
			int rem = num % 10;
			res = (res * 10) + rem;
			num = num/10;
		}
		System.out.println(temp);
		System.out.println(res);
		if(temp == res) {
			System.out.println("its palindrome");
		}
		else {
			System.out.println("its not a palindrome");
		}
	}

}
