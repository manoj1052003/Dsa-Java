package com.april;
import java.util.*;

public class numbers1 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner sc = new Scanner(System.in);
		System.out.println("enter the val of n");
		int n = sc.nextInt();
		for(int i=1; i<=n; i++) {
			if(i%2==1) {
				System.out.print(i+" ");
			}
			else {
				System.out.println(i);
			}
			
		}
	}

}
