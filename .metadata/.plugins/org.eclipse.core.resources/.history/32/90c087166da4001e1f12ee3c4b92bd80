package com.arrays;

public class binarysearch {
	
	static int searchbinary(int[] arr, int key) {
		
		int low= 0;
		int high= arr.length-1;
		
		while(low<=high) {
			
			int mid = (low+high)/2;
			if (key == arr[mid]) {
				return mid;
			}
			else if(key>arr[mid]) {
				low=mid+1;
			}
			else {
				high=mid-1;
			}
		}
		return -1;
		
	}

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		int [] arr = {1,3,5,7,9,23,34,45,56,67,78,90};
		int key = 56;
		int index = searchbinary(arr,key);
		System.out.println(index);
		

	}

}
