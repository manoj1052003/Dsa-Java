package com.august;

public class eleventhclas {
	
	public static int rec_reverse(int num,int res) {
		if(num==0) {
			return res;
		}
		return rec_reverse(num/10,(res*10)+num%10);
	}

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int num = 123;
		int res = rec_reverse( num, 0);
		System.out.println(res);
		
		
	}

}
