package com.march;

public class recfirst {
	
	public static int  print_num(int num) {
		if(num==10) {
			return 0;
		}
		System.out.println(num);
		return print_num(num-1);
		
	}

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int num = 5;
		int n = print_num(num);
		System.out.println(n);
		
	}

}
