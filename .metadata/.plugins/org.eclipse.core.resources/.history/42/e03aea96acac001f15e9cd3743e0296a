package com.sepnov;

public class class2 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		int k =1;     // k = 6
		while(k<=5) {
			System.out.println("hello");
			k++;
		}
		
		System.out.println("----------------------");
		
		int j = 1;
		do {
			System.out.println("hi");
			j++;
		}
		while(j<=5);
	}

}
