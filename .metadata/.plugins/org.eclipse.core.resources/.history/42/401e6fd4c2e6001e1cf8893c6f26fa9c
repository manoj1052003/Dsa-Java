package com.csr;


public class number {
	public static boolean check_prime(int num) {
		for (int i=2; i<=Math.sqrt(num);i++) {
			if (num%i==0) {
				return false;
			}	
		}
		return true;
	}

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int num = 17;
		boolean res=check_prime(num);
		if (res==true) {
			System.out.println(num);
		}
		
	}

}
