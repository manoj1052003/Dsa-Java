package com.sepnov;

import java.util.Arrays;
import java.util.Scanner;

public class num1 {

	public static void main(String[] args) {
		
		String s = "madam";
		String res = "";
		for(int i=s.length()-1; i>=0; i--) {
			char ch = s.charAt(i);
			res+=ch;
		}
		if(s == res) {
			System.out.println("its palindrome");
		}
		else {
			System.out.println("its not");
		}
		
		
	}

}
