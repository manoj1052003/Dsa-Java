package com.basicspro;

import java.util.Arrays;

public class firdtclass {
	
	public static void main(String[] args) {
		String s = "HeLlo";
		String res = "";
		for(int i=0; i<s.length(); i++) {
			char ch = s.charAt(i);
			if(ch >=97 && ch<=122) {
				res = res + (char)(ch-32);
			}
			else {
				res = res + ch;
			}
		}
	}
}
	
