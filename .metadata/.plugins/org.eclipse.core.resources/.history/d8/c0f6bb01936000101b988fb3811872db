package com.basic;

public class Logicalopr {

	public static void main(String[] args) {
		int num = 12321;
		int temp = num;
		int res = 0;
		while(num !=0 ) {
			int rem = num % 10;
			res = (res * 10) + rem;
			num /= 10;
		}
		System.out.println(temp);
		System.out.println(res);
		if(temp == res) {
			System.out.println("ita palindrome");
		}
		else {
			System.out.println("its not");
		}
		
	}

}
