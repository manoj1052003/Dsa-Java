package com.july24;
import java.util.*;
public class array1 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int[] arr = new int[5];
		System.out.println(Arrays.toString(arr));
		arr[0] = 10;
		System.out.println(Arrays.toString(arr));
		arr[4] = 50;
		System.out.println(Arrays.toString(arr));
		arr[6] = 50;
		System.out.println(Arrays.toString(arr));
	}

}
