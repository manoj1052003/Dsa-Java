package com.grooming;


public class kanini {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		String s = "pentagon";
		for(int i=0; i<s.length();i++) {
			System.out.print(s.charAt(i));
		}
		System.out.println();
		for(int i=s.length()-1;i>=0;i--) {
			System.out.print(s.charAt(i));
		}
		
		
		
	}

}
