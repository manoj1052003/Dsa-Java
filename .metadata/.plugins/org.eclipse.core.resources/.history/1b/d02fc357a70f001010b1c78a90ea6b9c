package com.basicspro;

public class firdtclass {

	public static void main(String[] args) {
		
		int i = 1;
		while(i<=5) {
			System.out.println("hi");
			i++;
		}
		System.out.println("-------------------------");
		int j = 1;
		do {
			System.out.println("hello");
			j++;
		}
		while(j<=5);
	}

}
