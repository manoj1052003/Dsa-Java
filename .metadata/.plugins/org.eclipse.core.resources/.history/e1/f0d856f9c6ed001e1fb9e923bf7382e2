package com.csr;

public class recprime {
	
	public static boolean prime(int num,int i) {
		if(num%i==0) {
			return false;
		}
		if(i==num) {
			return true;
		}
		return prime(num,i+1);
	}

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int num = 7;
		int i = 2;
		boolean res = prime(num,i);
		System.out.println(res);

	}

}
