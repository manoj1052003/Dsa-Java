package com.sepnov;

public class Lastclass {

	public static void main(String[] args) {
		
		String day = "monday";
		switch(day) {
		case "monday":
			System.out.println("its boring day");
			
		case "tuesday":
			System.out.println("again its boring day");
			
		case "wednesday":
			System.out.println("no special day");
			
		default:
			System.out.println("boring day");
		}

	}

}
