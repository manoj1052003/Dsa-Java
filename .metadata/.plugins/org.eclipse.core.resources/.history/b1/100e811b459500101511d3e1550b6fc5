package july;

public class class5 {

	public static void main(String[] args) {
		String s = "pe$n ta@gonspace";
		int c_v = 0;
		int c_c = 0;
		int c_s = 0;
		int c_spl = 0;
		for(int i = 0; i < s.length(); i++) {
			char ch = s.charAt(i);
			if((ch >= 97 && ch <= 122) || ch == 32) {
				if(ch == 'a' || ch == 'e' || ch == 'i' || ch == 'o' || ch == 'u') {
					c_v++;
				}
				else if(ch == 32) {
					c_s++;
				}
				else {
					c_c++;
				}
			}
			else {
				c_spl++;
			}
				
		}
		System.out.println(c_v);
		System.out.println(c_s);
		System.out.println(c_c);
		System.out.println(c_spl);

	}

}
