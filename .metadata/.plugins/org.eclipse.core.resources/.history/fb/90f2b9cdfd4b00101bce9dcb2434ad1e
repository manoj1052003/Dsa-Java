package com.numbers;


public class classmeth {
	
	public static int count_digit(int num) {
		int count = 0;
		while(num != 0 ) {
			num/=10;
			count++;
		}
		return count;
	}
	
	public static int arm(int num) {
		int count = count_digit(num);
		int res = 0;
		while(num != 0) {
			int rem = num % 10;
			res = (int)Math.pow(res, rem) + res;
		}
	}

	public static void main(String[] args) {
		
		int num = 153;
		int digit = count_digit(num);
		System.out.println(digit);
		
	
		
		
	}
}
