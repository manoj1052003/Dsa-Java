package com.opr;

class Node{
	int data;
	Node next;
	public Node(int data) {
		this.data = data;
		this.next = null;
	}
}




public class Class6 {
	
	public void print_LL(Node head) {
		if(head == null) {
			System.out.println("ll is empty");
		}
		else {
			Node curr = head;
			while(curr!=null) {
				System.out.println(curr.data);
				curr = curr.next;
			}
		}
	}

	public static void main(String[] args) {
		
		Node n1 = new Node(10);
		Node n2 = new Node(20);
		Node n3 = new Node(30);
		Node n4 = new Node(40);
		n1.next = n2;
		n2.next = n3;
		n3.next = n4;
		
		Node head = n1;
		Class6 c = new Class6();
		c.print_LL(head);

	}

}
