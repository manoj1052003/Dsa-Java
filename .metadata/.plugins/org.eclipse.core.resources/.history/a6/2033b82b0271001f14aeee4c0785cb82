package com.june19;

public class twelethclass {
	
	public static int hcf_gabriel_lame(int a, int b) {
		while(a!=0 && b!=0) {
			if(a>b) {
				a = a%b;
			}
			else {
				b = b%a;
			}
		}
		return (a!=0)?a:b;
	}

	public static void main(String[] args) {
		int a = 24;
		int b = 18;
		int res = hcf_gabriel_lame(a,b);
		System.out.println(res);
		

	}

}
