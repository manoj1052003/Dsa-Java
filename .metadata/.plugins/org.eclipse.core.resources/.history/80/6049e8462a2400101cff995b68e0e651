package com.sepnov;

import java.util.Arrays;
import java.util.Scanner;

public class num1 {
	
	public static String rev(String s) {
		String res = "";
		for(int i=s.length()-1; i>=0; i--) {
			char ch = s.charAt(i);
			res+=ch;
		}
		return res;
	}

	public static void main(String[] args) {
		
		String s = "pentagon";
		
	}

}
