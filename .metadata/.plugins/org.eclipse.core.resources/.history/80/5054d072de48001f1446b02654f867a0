package com.june;
import java.util.*;
public class splitttt {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		String s = "ABDEF";
		String res = "";
		for (int i=0; i<s.length(); i++) {
			char c  = s.charAt(i);
			res = c + res;
		}
		System.out.println(res);
		
	}

}
