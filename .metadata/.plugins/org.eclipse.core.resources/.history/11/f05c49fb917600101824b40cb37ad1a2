package com.basic;

import java.util.Arrays;
import java.util.Scanner;

public class Class3 {

	public static void main(String[] args) {
		
		String s = "pEntagon";
		int v_count = 0;
		for(int i = 0; i<s.length(); i++) {
			char ch = s.charAt(i);
			if(ch == 'a' || ch == 'e' || ch == 'i'|| ch == 'o'|| ch == 'u') {
				v_count++;
			}
		}
		System.out.println(v_count);
		
	
	}

}
