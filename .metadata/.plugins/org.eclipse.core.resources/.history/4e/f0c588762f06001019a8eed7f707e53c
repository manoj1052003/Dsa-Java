package com.jan26;

import java.util.Arrays;

public class class14num4 {

	public static void main(String[] args) {
		
		String s = "AabcDDD";
		for(int i=0; i<s.length(); i++) {
			char ch = s.charAt(i);
			if(ch>=65 && ch<=90) {
				System.out.print(ch);
			}
			else {
				char nch = (char)(ch -32);
				System.out.print(nch);
			}
		}
		
		
	}

}
