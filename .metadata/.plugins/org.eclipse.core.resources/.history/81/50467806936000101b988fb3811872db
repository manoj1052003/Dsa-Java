package com.basic;

public class Logicalopr {
	
	public static int reverse(int num) {
		int res = 0;
		while(num !=0 ) {
			int rem = num % 10;
			res = (res * 10) + rem;
			num /= 10;
		}
		return res;
	}

	public static void main(String[] args) {
		int num = 12321;
		int res = reverse(num);
		if(res == num) {
			System.out.println("ita palindrome");
		}
		else {
			System.out.println("its not");
		}
		
	}

}
