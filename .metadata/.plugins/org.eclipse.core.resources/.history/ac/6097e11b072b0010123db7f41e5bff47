package com.sepnov;

public class Lastclass {

	public static void main(String[] args) {
		String day = "monday";
		switch(day) {
		case "monday":
			System.out.println("monday");
			break;
		case "tuesday":
			System.out.println("tuesday");
			break;
		case "thursday":
			System.out.println("saturday");
			break;
		case "sunday":
			System.out.println("sunday");
			break;
		default:
			System.out.println("invld");
		}
	}

}
