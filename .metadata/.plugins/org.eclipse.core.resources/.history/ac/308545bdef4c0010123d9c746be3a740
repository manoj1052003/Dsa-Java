package com.numbers;


public class classmeth { 
	
	public static int sum_digit(int a) {//0
		int res = 0;                     //9
		while(a != 0 ) {
			int rem = a % 10;            //rem = 8
			res = res + rem;
			a = a / 10;
		}
		return res;
	}
	
	public static void main(String[] args) {
		int num = 9;
		int a = (num * num);
		int res = sum_digit(a);
		if(num ==  res) {
			System.out.println("neon");
		}
		else {
			System.out.println("its not");
		}
		
	}
}
