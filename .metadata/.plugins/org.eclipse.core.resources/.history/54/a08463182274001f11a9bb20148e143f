package com.june19;

public class thirteen {
	
	public static void rec_fibo(int num, int a, int b,int sum) {
		if(num==0) {
			System.out.println(sum);
			return ;
		}
		System.out.println(a);
		rec_fibo(num-1, b, a+b,sum+a);
		
	}
	
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int num = 8;
		rec_fibo(num,0,1,0);
		
	}

}
