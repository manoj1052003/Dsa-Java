package com.sepnov;

public class class17 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int [] arr = {1,45,8,63,63};
		int max1 = 0; //8
		int max2 = 0; //4
		if(arr[0]>arr[1]) {
			max1 = arr[0];
			max2 = arr[1]; 
		}
		else {
			max1 = arr[1];
			max2 = arr[0];
		}
		for(int i=2; i<arr.length; i++) {
			if(arr[i]>max1) {
				max2 = max1;
				max1 = arr[i];
			}
			else if(arr[i]>max2) {
				max2 = arr[i];
			}
		}
		System.out.println(max2);
	}

}
