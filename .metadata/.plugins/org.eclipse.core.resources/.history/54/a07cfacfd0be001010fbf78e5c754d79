package com.graph;


class AdjMat{
	
	int Nodes;
	int [] [] arr;
	
	AdjMat(int Nodes) {
		arr = new int [Nodes][Nodes];
		this.Nodes = Nodes;
	}
	
	public void add_edge(int src, int des) {
		arr[src][des] = 1;
		arr[des][src] = 1;
	}
	
	public void print() {
		for(int i=0; i<arr.length; i++) {
			for(int j=0; j<arr.length; j++) {
				System.out.print(arr[i][j]+" ");
			}
			System.out.println();
		}
	}
	
}

public class Second {

	public static void main(String[] args) {
		AdjMat am = new AdjMat(5);
		

	}

}
