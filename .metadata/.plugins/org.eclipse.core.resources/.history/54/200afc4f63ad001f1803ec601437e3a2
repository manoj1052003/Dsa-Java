package com.sepnov;

public class class3 {

	public static void main(String[] args) {
		// switch
		int day = 4;
		switch(day) {
		
		case 1:
			System.out.println("shiva");
			
		case 2:
			System.out.println("no hair cut");
			
		case 3: 
			System.out.println("clr dress");
			
		case 4:
			System.out.println("sai baba");
			
		case 5:
			System.out.println("black friday");
			
		case 6:
			System.out.println("boss");
			
		case 7:
			System.out.println("pub");
		}
		
	}

}
