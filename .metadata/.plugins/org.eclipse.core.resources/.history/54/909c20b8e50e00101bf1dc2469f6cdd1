package com.grooming;

import java.util.Scanner;

public class kanini {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		String s = "malayalam";
		String res = "";
		for(int i=s.length()-1;i>=0;i--) {
			res = res + s.charAt(i);
		}
		
		System.out.println(res);
		
	}

}
