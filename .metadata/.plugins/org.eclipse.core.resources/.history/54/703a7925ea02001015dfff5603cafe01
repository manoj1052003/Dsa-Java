package com.jan26;

public class class16num6 {
	
	public static int add_digit(int num) {//14
		int res = 0;
		while(num!=0) {
			int rem = num % 10;
			res = res + rem;
			num = num / 10;
		}
		return res;
	}
	
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int num = 14;
	}

}
