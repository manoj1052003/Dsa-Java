package com.pattern;

public class todayclspat {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int rows = 9;
		int cols = 9;
		for (int i=1; i<=rows; i++) {
			for (int j=1; j<=cols; j++) {
				if(i==(rows)/2 || j==(cols)/2 ||i==1&& j>=5|| i==rows&& cols<=5|| j==1&& i<=5||j==9&& i>=5) {
					System.out.print("*"+" ");
				}
				else {
					System.out.println(" "+" ");
				}
			}
			System.out.println();
		}
	}

}
