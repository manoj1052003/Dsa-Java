package com.sepnov;

public class Lastclass {

	public static void main(String[] args) {
		int day = 12;
		switch(day) {
		case 1:
			System.out.println("monday");
			break;
		case 2:
			System.out.println("tuesday");
			break;
		case 3:
			System.out.println("saturday");
			break;
		case 4:
			System.out.println("sunday");
			break;
		default:
			System.out.println("invld");
		}
	}

}
