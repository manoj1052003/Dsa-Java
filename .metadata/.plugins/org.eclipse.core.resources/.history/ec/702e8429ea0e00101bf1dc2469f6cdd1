package com.grooming;

public class kanini {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		String s = "pent  agon";
		int v_c = 0;
		int c_c = 0;
		for(int i=0; i<s.length(); i++) {
			char ch = s.charAt(i);
			if(ch=='a' || ch=='e' || ch=='i' || ch=='o' || ch=='u') {
				v_c++;
			}
			else {
				c_c++;
			}
		}
		System.out.println(v_c);
		System.out.println(c_c);
		
		
		
		
	}

}
