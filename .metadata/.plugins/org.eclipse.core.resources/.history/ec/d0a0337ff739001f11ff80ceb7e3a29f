package com.june;

public class num6 {
	
	public static int find_fact(int num) {
		if(num==1) {
			return 1;
		}
		return find_fact(num-1)*num;
	}

	public static void main(String[] args) {
		// TODO Auto-generated method stub
			int num = 5;
			int res = find_fact(num);
			System.out.println(res);
			
	}

}
