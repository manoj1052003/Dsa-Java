package com.jan26;

public class class14num4 {
	
	public static int l_search(int [] arr, int key) {
		for(int i=0; i<arr.length; i++) {
			if(arr[i] == key) {
				return i;
			}
		}
		return -1;
	}
	
	public static void main(String[] args) {
		int arr [] = {-7,-5,-8,-4,-9};
		int key = -4;
		
	}

}
