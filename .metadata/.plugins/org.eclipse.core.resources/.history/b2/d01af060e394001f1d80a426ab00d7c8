package workshop;
import java.util.*;
public class day8 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		String s = "pentagonspace";
		int count = 0;
		for(int i=0; i<s.length(); i++) {
			char c = s.charAt(i);
			if(c=='a'|| c=='e'|| c=='i'|| c=='o'|| c=='u') {
				System.out.println(c);
				count++;
			}
		}
		System.out.println("no of vowels are : "+count);

	}

}
