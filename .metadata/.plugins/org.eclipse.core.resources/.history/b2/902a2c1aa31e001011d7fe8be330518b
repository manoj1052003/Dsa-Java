package com.kiran;

public class today {

	public static void main(String[] args) {
		int rows = 5;
		for(int i=1; i<=rows; i++) {
			for(int j=1; j<=(rows*2)-1; j++) {
				if(i+j==rows+1 || j-i==rows-1) {
					System.out.print(i+" ");
				}
				else {
					System.out.print(""+" ");
				}
				
			}
			System.out.println();
		}
	}
}
