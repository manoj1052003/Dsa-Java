package com.june;
import java.util.*;
public class stringbasic1 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		String s = "pentagon";
		for(int i = 0; i<s.length(); i++) {
			char c = s.charAt(i);
			System.out.print(c);
		}
		System.out.println();
		char [] arr = s.toCharArray();
		System.out.println(arr);
		System.out.println(Arrays.toString(arr));
		
	}

}
