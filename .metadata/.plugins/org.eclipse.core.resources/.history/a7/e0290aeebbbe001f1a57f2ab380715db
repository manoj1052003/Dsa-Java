 package com.sepnov;

public class class19 {

	public static void main(String[] args) {
		int [] arr = {-9,-6,8,5,-4,6,-8,7};
		int max = arr[0];
		for(int i=0; i<arr.length; i++) {
			int sum = 0;
			for(int j=i; j<arr.length; j++) {
				sum+=arr[j];
				if(sum>max) {
					max= sum;
				}
			}
		}
		System.out.println(max);
	}
}
