package com.opr;

import java.util.Arrays;
import java.util.Scanner;

public class Pattern3 {
	
	 public static void main(String[] args) {
		 //s.toCharArray();
		 String s = "hello";
		 char [] arr = new char[s.length()];
		 for(int i=0; i<s.length(); i++) {
			 arr[i] = s.charAt(i);
		 }
		 
		 System.out.println(Arrays.toString(arr));
	}

}
