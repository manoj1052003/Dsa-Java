package com.sepnov;

public class Lastclass {
	
	
	public static void find_ans(String s) {
		String res = "";
		for(int i=0; i<s.length(); i+=2) {
			char ch = s.charAt(i+1);
			int num = Character.getNumericValue(ch);
			while(num!=0) {
				res = res + s.charAt(i);
				num--;
			}
			
		}
		
	}

	public static void main(String[] args) {
		String s = "a2b5c3";
		find_ans(s);
		
		
		

	}

}
