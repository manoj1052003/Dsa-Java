package com.onlinejavapro;

public class class5 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int rows = 9;
		int k = 1;
		for(int i=1; i<=rows; i++) {
			for(int j=1; j<=i; j++) {
				if(k%2==0) {
					System.out.print(0+" ");
				}
				else {
					System.out.print(1+" ");
				}
				k++;
			}
			System.out.println();
		}
	}

}
