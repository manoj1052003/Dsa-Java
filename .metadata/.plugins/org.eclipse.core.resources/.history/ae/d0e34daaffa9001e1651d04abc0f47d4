package com.novdec;
import java.util.Scanner;

public class countdigit {

	public static void main(String[] args) {
		
		int num = 153;
			int res=0;
			int n=num;
			int temp=num;
			int count=0;
			while(n>0) {
				n=n/10;
				count+=1;
			}
			while(num>0)
			{
				int rem=num%10;
				res=(int)(Math.pow(rem,count))+res;
				num=num/10;
			}
			if(res==temp)
			{
				System.out.println(res);
			}


	
	}
}
		
