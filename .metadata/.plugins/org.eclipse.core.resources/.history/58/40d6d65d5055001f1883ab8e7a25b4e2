package com.june;

public class fibrec {
	
	public static void find_fib(int num,int a,int b) {
		if (num<=1) {
			return; 
		}
		System.out.println(a);
		find_fib(num-1,b,a+b);
		
	}

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int num = 8;
		find_fib(num,0,1);
		System.out.println(res);
		
	}

}
