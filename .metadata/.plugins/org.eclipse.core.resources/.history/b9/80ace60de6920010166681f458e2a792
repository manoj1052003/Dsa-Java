package july;

public class class2 {

	public static void main(String[] args) {
		int rows = 5;
		int k = 1;
		int u = 65;
		int l = 97;
		for(int i = 1; i <= rows; i++) {
			for(int j=1; j<=i; j++) {
				if(k%2==0) {
					System.out.print((char)l+" ");
				}
				else {
					System.out.print((char)u+" ");
				}
				l++;
				u++;
				k++;
			}
			System.out.println();
		} 

	}

}
