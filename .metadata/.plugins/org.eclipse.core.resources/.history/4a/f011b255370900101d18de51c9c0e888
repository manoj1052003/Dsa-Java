package com.jan26;

import java.util.Arrays;

public class class14num4 {
	
	public static void sub_array(int [] arr, int s) {
		int s_length = arr.length;
		for(int i=0; i<arr.length; i++) {
			int sum = 0;
			for(int j=i; j<arr.length; j++) {
				sum = sum + arr[j];
				if(sum>=s) {
					if(j-i+1  < s_length) {
						s_length = j-i+1;
					}
				}
			}
		}
		if(s_length==arr.length) {
			System.out.println(0);
		}
		else {
			System.out.println(s_length);
		}
	}
	
	
	public static void main(String[] args) {
		int [] arr = {2,3,1,2,4,3};
		int s = 7;
		
	}

}
