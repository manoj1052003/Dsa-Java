package com.june19;

public class ninthclass {
	
	public static int reverse(int num) {
		int res = 0;
		while(num!=0) {
			int rem = num % 10;
			res = (res * 10) + rem;
			num = num / 10;
		}
		return res;
	}

	public static void main(String[] args) {
		int num = 111;
		int rev = reverse(num);
		System.out.println(num);

		if( num == rev) {
			System.out.println("its palin");
		}
		else {
			System.out.println("its not");
		}
		
	}
}
