package july;

import java.util.Arrays;

public class class4 {

	public static void main(String[] args) {
		int[] arr = { 4, -1, 2, 2, 1, -4, 6 };
		int k = 7;
		for (int i = 0; i < arr.length; i++) {
			for(int j=i; j<arr.length; j++) {
				for(int l=i; l<=j; l++) {
					System.out.print(arr[l]);
				}
				System.out.println();
			}
		}

	}

}
