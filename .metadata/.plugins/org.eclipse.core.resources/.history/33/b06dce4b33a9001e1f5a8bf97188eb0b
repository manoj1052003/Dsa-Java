package com.novdec;

public class revrese {

	public static void main(String[] args) {
		int num=020;
		int res=0;
		int ex=num;
		while(num>0)
		{
			int rem=num%10;
			res=(res*10)+rem;
			num=num/10;
		}
		if(ex==res) {
		System.out.println("palindrome");

	}
		else
		{
			System.out.println("not a palindrome");
		}
	}
}
