package com.basics;

import java.util.Arrays;
import java.util.Scanner;

public class Class2 {

	public static void main(String[] args) {
		String s = "malayalam";
		String res = "";
		for(int i=s.length()-1; i>=0; i--) {
			res += s.charAt(i);
		}
		if(s == res) {
			System.out.println("palin");
		}
		else {
			System.out.println("not");
		}
	}
}
