package com.pattern;

public class impresscrush {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		int rows = 6;
		int col = 7;
		
		for (int i =0; i<rows; i++) {
			
			for (int j = 0; j < col; j++) {
				
				if ((i==1 && j%3==0) || (i==0 && j%3!=0) || (j-i==4) || (i+j==8)){
					
					System.out.print("*"+" ");
				}
				
				else {
					
					System.out.print(" "+" ");
				}
				
			}
			
			System.out.println();
		}
	
	}

}
