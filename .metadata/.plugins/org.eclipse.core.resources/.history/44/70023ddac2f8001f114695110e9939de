package com.jan26;

public class class4patt2 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int i = 1;
		do {
			System.out.println("hi");
			i++;
		}
		while(i<=5);
		
		
		System.out.println("-----------------");
		
		int j=1;
		while(j<=5) {
			System.out.println("hello");
			j++;
		}
	}
	
	
	

}
