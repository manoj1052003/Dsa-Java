package com.stack;

class Node{
	int data;
	Node next;
	
	Node(int data){
		this.data = data;
		this.next = null;
	}
}

class LLStack{
	Node head;
	
	public void push(int data) {
		Node temp = new Node(data);
		temp.next = head;
		head = temp;
		System.out.println(temp+ "pushed to stack");
	}
	
	public void pop() {
		if(head == null) {
			System.out.println("stack underflow exc");
			return;
		}
		int last = head.data;
		head = head.next;
		System.out.println(last + "is popped off");
	}
	
	
}


public class LinkedListStack {

	public static void main(String[] args) {
		LLStack lls = new LLStack();
		lls.push(10);
		lls.push(20);
		lls.pop();

	}

}
