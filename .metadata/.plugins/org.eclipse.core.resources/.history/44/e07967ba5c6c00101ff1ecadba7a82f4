package com.basicop;

import java.util.Arrays;
import java.util.Scanner;

public class Class1assign {

	public static int [] b_sort(int [] res) {
		for(int i=0; i<res.length; i++) {
			for(int k=0; k<res.length-1; k++) {
				if(res[k] < res[k+1]) {
					int temp = res[k];
					res[k] = res[k+1];
					res[k+1] = temp;
				}
			}
		}
		return res;
	}
	
	public static void r_anti(int [] arr) {
		int temp = arr[0];
		for(int i=1; i<arr.length; i++) {
			arr[i-1] = arr[i];
		}
		arr[arr.length-1] = temp;
	}
	
	public static void r_clock(int [] arr) {
		int temp = arr[arr.length-1];
		for(int i=arr.length-2; i>=0;  i--) {
			arr[i+1] = arr[i];
		}
		arr[0] = temp;
	}
	
	public static void main(String[] args) {
		int [] arr = {10,20,30,40,50,60};
		int [] res = {10,20,30,40,50,60};
		System.out.println(Arrays.equals(arr, res));
		
		
		
			
	}

}
