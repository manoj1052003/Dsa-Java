package com.sepnov;

public class class13 {
	public static int add_digit(int num) {
		int res = 0;
		while(num!=0) {
			int rem = num % 10;
			res = res + rem;
			num = num / 10;
		}
		return res;
	}
	

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int num = 10;
		int num1 = num+1;
		while(add_digit(num)*2 != add_digit(num1)) {
			num1++;
		}
		System.out.println(num1);
		
		
	}

}
