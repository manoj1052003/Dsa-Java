package com.novdec;

public class sat {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int num = 15;
		
		boolean res = true;
		
		for (int i = 2; i < num; i++) {
			
			if (num % i == 0) {
				res = false;
				break;
			
			
		}
		
		System.out.print(res);

	}

}
