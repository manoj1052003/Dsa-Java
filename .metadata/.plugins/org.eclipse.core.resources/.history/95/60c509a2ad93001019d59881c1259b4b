package july;

public class class3 {
	
	public static int sum_digit(int num) {
		int sum = 0;
		while(num != 0) {
			int rem = num % 10;
			sum = sum + rem;
			num = num / 10;
		}
		return sum;
	}

	public static void main(String[] args) {
		int num = 10;
		int res = num + 1;
		while(sum_digit(num)*2 != sum_digit(res)) {
			res++;
		}
		System.out.println(res);
	}

}
