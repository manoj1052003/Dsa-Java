package com.march;

public class satfact {
	
	public static int fact(int num) {
		int res = 1;
		for (int i =1 ; i<=num; i++) {
			res = res * i;
		}
		return res;
	}

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int num = 5;
		int res = fact(num);
		System.out.println(res);
	}

}
