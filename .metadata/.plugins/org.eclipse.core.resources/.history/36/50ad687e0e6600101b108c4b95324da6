package com.basicop;

public class Class1assign {
	
	public static int sqr_sum_digits(int num) {
		int sum = 0;
		while(num!=0) {
			int rem = num % 10;
			sum = sum + (rem * rem);
			num = num / 10;
		}
		return sum;
	}

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int num = 18;
		int res = sqr_sum_digits(num);
		System.out.println(res);
		
	}

}
