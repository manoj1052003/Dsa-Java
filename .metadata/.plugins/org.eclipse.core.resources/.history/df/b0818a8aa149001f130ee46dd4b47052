package com.june;

public class substringggg1 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		String s = "abc";
		for(int i=0; i<s.length(); i++) {
			String res = "";
			for(int j=i; j<s.length(); j++) {
				//System.out.println(s.substring(i, j+1));
				res = res + s.charAt(j);
				System.out.println(res);
			}
		}
	}

}
