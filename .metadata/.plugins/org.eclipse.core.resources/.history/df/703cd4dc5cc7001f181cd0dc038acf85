package com.sepnov;
import java.util.*;
public class class23 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		String s = "madam";
		for(int i=0; i<s.length(); i++) {
			String res = "";
			for(int j=i; j<s.length(); j++) {
				res = res + s.charAt(j);
				System.out.println(res);
			}
		}
	}

}
