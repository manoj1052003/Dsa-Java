package com.basicop;


public class Class1assign {
	

	public static void main(String[] args) {
		int [] arr = {10,20,30,40,50};
		int sum = 0;
		for(int i=0; i<arr.length; i++) {
			for(int j=i; j<arr.length; j++) {
				for(int k=i; k<=j; k++) {
					System.out.print(arr[k] + " ");
					sum+=arr[k];
				}
				System.out.println("sum "+ sum);
				System.out.println();
			}
		}
		
		
	}

}
