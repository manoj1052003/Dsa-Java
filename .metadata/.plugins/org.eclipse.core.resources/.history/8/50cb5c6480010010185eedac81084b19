package com.jan26;

public class class14num4 {
	
	public static int rev(int num) {
		int res = 0;
		while(num!=0) {
			int rem = num % 10;
			res = (res * 10) + rem;
			num = num / 10;
		}
		return res;
	}

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int num = 6363;
		int res = rev(num);
		if(num == res) {
			System.out.println("its palindrome");
		}
		else {
			System.out.println("its not a palindrome");
		}
		
	}

}
