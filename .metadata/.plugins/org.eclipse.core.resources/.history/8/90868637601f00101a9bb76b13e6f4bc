package com.sepnov;
import java.util.Random;

public class num1 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Random ra = new Random();
		long num = ra.nextLong();
		
		if(num<0) {
			num = num * -1;
		}
		//System.out.println(num);
		
		int len = 0;
		long res = 0;
		while(len<15) {
			long rem = num%10;
			res = (res * 10) + rem;
			num = num / 10;
			len++;
		}
		System.out.println(res);
		
		
	}

}
