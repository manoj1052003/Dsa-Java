package com.june19;

public class thirteen {
	public static int rec_rev(int num,int res) {
		if(num==0) {
			return res;
		}
		return rec_rev(num/10, (res*10)+num%10);
	}
	

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int num = 1234;
		int res = 0;
		rec_rev(num, res);
		
		
	}

}
