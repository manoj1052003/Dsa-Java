package com.august;
import java.util.*;
public class anagram {


	public static void main(String[] args) {
		// TODO Auto-generated method stub
		String s = "malayalam";
		String res = "";
		for(int i=0; i<s.length(); i++) {
			char ch = s.charAt(i);
			res = ch + res;
		}
		if(res == s) {
			System.out.println("palindrome");
		}
		else {
			System.out.println("its not");
		}
	}
}
