package com.grooming;
import java.util.*;
public class hashhhhmapppp {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int []  arr = {2,2,3,2};
		int [] res  =new int[10];
		for(int val : arr) {
			if(res[val]==0) {
				res[val] = 1;
			}
			else {
				res[val]+=1;
			}
		}
		System.out.println(Arrays.toString(res));
	}

}
