package com.basics;

public class Class2 {
	
	public static int rev(int num) {
		int res = 0;
		while(num != 0) {
			int rem = num % 10;
			res = (res * 10) + rem;
			num = num / 10;
		}
		return res;
	}

	public static void main(String[] args) {
		int rows = 6;
		int k = 1;
		for(int i=1; i<=rows; i++) {
			for(int j=1; j<=i; j++) {
				if(k == rev(k)) {
					System.out.print(k+" ");
				}
				k++;
			}
			System.out.println();
		}
	}
}
