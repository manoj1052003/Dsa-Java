package com.basicop;

import java.util.Arrays;
import java.util.Scanner;

public class Class1assign {

	public static void main(String[] args) {
		int [] arr = {45,18,17,63,7,39};
		int max = 0;
		for(int i=0; i<arr.length; i++) {
			if(arr[i] > max) {
				max = arr[i];
			}
		}
		System.out.println(max);
	}

}
