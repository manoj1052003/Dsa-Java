package com.jan26;

public class class14num4 {
	
	public static int c_digit(int num) {
		int digit = 0;
		while(num!=0) {
			num/=10;
			digit+=1;
		}
		return digit;
	}
	
	public static int check_arm(int num) {
		int digit = c_digit(num);
		int res = 0;
		while(num!=0) {
			int rem = num % 10;
			res = (int)Math.pow(rem, digit) + res;
			num/=10;
		}
		return res;
	}

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int num = 153;
		int res = check_arm(num);
		if(res == num) {
			System.out.println("armstrong");
		}
		else {
			System.out.println("its not");
		}
		
		
	}

}
