package com.numbers;

public class classmeth {

	public static void main(String[] args) {
		int num = 12321;
		int res = 0;
		int temp = num;
		while (num != 0) {
			int rem = num % 10;
			res = (res * 10) + rem;
			num = num / 10;
		}
		System.out.println(temp);
		System.out.println(res);
		if(temp == res) {
			System.out.println("its palindrome");
		}
		else {
			System.out.println("its not");
		}
	}
}
