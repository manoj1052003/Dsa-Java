package com.numbers;

public class hcfeuclid {
	
	static int find_hcf(int a, int b) {
		
		while(a>0 && b>0) {
			
			if (a > b) {
				
				a= a -b;
				
			}
			else {
				
				b=b-a;
			}
			
		}
		
		if (a != 0) {
			return a;
		}
		else {
			return b;
		}
		
				
	}

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		int a = 4;
		int b = 10;
		int res = (find_hcf(a, b));
		System.out.println(res);
		

	}

}
