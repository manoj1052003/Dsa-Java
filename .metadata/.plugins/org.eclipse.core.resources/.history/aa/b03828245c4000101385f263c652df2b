package com.opr;

public class may1 {

	public static void main(String[] args) {
		int day = 3;
		switch(day) {
		case 1:
			System.out.println("its monday");
			
		case 2:
			System.out.println("match day");
			
		case 3:
			System.out.println("celebration day");
			
		case 4:
			System.out.println("next sala cup namde");
		
		case 5:
			System.out.println("punjab");
		}
		

	}

}
