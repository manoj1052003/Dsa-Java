package com.opr;

import java.util.Arrays;

public class Pattern3 {
	
	public static void main(String[] args) {
		
		int [][] arr = {{1,2,3},
						{4,5,6},
						{7,8,9}}; 
		int sum1 = 0;
		int sum2 = 0;
		for(int i=0; i<arr.length; i++) {
			for(int j=0; j<arr.length; j++) {
				if(i==j) {
					sum1 = sum1 + arr[i][j];
				}
				if(i+j==arr.length-1) {
					sum2 = sum2 + arr[i][j];
				}
			}
		}
		if(sum1 > sum2) {
			System.out.println(sum1-sum2);
		}
		else {
			System.out.println(sum2-sum1);
		}
	}

}
