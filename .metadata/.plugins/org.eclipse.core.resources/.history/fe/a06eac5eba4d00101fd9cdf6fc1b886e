package com.numbers;


public class classmeth { 
	
	public static int num_sum(int num) {
		int sum = 0;
		while(num != 0) {
			int rem = num % 10;
			sum = sum + rem;
			num = num / 10;
		}
		return sum;
	}
	
	public static void main(String[] args) {
		int num = 14;
		int res = num + 1;
		while(num_sum(num)*2 != num_sum(res)) {
			res++;
		}
		System.out.println(res);
	}
}
