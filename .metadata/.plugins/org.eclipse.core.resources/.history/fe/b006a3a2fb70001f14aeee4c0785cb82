package com.june19;

public class twelethclass {
	
	public static int hcf_euclied(int a, int b) {
		while(a!=b) {
			if(a>b) {
				a = a-b;
			}
			else {
				b = b-a;
			}
		}
		return a;
	}

	public static void main(String[] args) {
		int a = 18;
		int b = 24;
		int res = hcf_euclied(a,b);
		System.out.println(res);
		

	}

}
