package com.timecom;

public class first {

	public static void main(String[] args) {
		int a = 24;
		int b = 12;
		
		while(a != 0 && b != 0) {
			if(a > b) {
				a = a % b;
			}
			else {
				b = b % a;
			}
		}
		if(a !=0) {
			System.out.println(a);
		}
		else {
			System.out.println(b);
		}
		
	}
}
