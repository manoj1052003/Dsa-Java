package com.onlinejavapro;

public class class10 {
	
	public static int f_fact(int num) {
		int sum = 0;
		for(int i=1; i<num ; i++) {
			if(num%i==0) {
				sum = sum + i;
			}
		}
		return sum;
	}
	
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int num = 6;
		int res = f_fact(num);
		if(num == res) {
			System.out.println("perfect");
		}
		else {
			System.out.println("not");
		}
		
	}

}
