package com.april;

public class armstrong {
	
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int rows = 5;
		int k=1;
		int l = 96;
		int u = 64;
		for(int i=1; i<=rows; i++) {
			for(int j=1; j<=i; j++) {
				if(k%2==0) {
					System.out.print((char)(l+i)+" ");
				}
				else {
					System.out.print((char)(u+i)+" ");
				}
				k++;
			}
			System.out.println();
		}
	}

}
