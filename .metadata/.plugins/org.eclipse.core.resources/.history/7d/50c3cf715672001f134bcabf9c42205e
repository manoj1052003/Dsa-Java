package com.august;

public class eleventhclas {


	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int rows = 5;
		for(int i=1; i<=5; i++) {
			for(int j=1; j<=5; j++) {
				if(i==j || i+j==rows+1) {
					System.out.print(j+" ");
				}
				else {
					System.out.print(" "+" ");
				}
			}
			System.out.println();
		}
		
		
		
		
	}

}
