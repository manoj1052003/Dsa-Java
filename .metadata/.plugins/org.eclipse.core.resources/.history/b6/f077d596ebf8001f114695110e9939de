package com.jan26;

public class class4patt2 {
	
	public static int reverse_num(int num) {
		int res = 0;
		while(num != 0) {
			int rem = num % 10;
			res = (res * 10) + rem;
			num = num/10;
		}
		//System.out.println("num inside the method "+ num);
		return res;
	}

	public static void main(String[] args) {
		for(int i=1; i<=10000; i++) {
			int num = i;
			int res = reverse_num(num);
			//System.out.println("num outside the method "+ num);
			if(num == res) {
				System.out.println(num);
			}
		}
		

	}

}
