package com.august;

public class eleventhclas {
	
	public static int reverse(int num) {
		int res = 0;
		while(num!=0) {
			int rem = num % 10;
			res = (res * 10)+rem;
			num= num / 10;
		}

		return res;
	}

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int count = 0;
		int sum = 0;
		for(int i=1; ; i++) {
			int num = i;
			int res = reverse(num);
			if(num==res) {
				sum= sum+num;
				count++;
			}
			if(count==100) {
				System.out.println(sum);
				break;
			}
		}
		
		
	}

}
