package com.jan26;

import java.util.Arrays;

public class class14num4 {
	
	public static String con_upp(String s) {
		String res = "";
		for(int i=0; i<s.length(); i++) {
			if(i==0) {
				char nch = (char)(s.charAt(i)-32);
				res = res + nch;
			}
			else {
				res = res + s.charAt(i);
			}
		}
		return res;
	}
	
	public static void main(String[] args) {
			String s = "ee sala cup namde";
			String []  arr = s.split(" ");
			//System.out.println(Arrays.toString(arr));
			for(int i=0; i<arr.length; i++) {
				arr[i]
			}
		
	}

}
