package com.numbers;

import java.util.Arrays;

public class classmeth {
	
	public static int print_fact(int n) {
		int sum = 0;
		for(int i=1; i<n; i++) {
			if(n%i==0) {
				sum = sum + i;
			}
		}
		return sum;
	}

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int n = 6;
		int sum = print_fact(n);
		if(sum == n) {
			System.out.println("its perfect");
		}
		else {
			System.out.println("its not");
		}
			
	}
}
