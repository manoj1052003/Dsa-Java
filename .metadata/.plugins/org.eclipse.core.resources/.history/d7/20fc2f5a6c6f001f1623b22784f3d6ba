package com.june19;

public class tenthclass {
	
	public static int c_digit(int num) {
		int count = 0;
		while(num!=0) {
			num = num / 10;
			count++;
		}
		return count;
	}
	
	public static int c_arm(int num) {
		int digit = c_digit(num);
		int res = 0;
		while(num!=0) {
			int rem = num % 10;
			res = (int)Math.pow(rem, digit)+res;
			num/=10;
		}
		return res;
	}

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int count = 0;
		for(int i=1; i<=10000; i++) {
			int num = i;
			int res = c_arm(num);
			if(num==res) {
				System.out.println(num);
				count++;
			}
			if(count==12) {
				break;
			}
		}
		
	}

}
