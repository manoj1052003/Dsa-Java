package workshop;
import java.util.*;
public class day7 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int [] arr = {12,23,34,45,56};
		System.out.println(Arrays.toString(arr));
		for(int k=1; k<=400; k++) {
			int temp = arr[0];
			for(int i=1; i<arr.length; i++) {
				arr[i-1] = arr[i];
			}
			arr[arr.length-1] = temp;
		}
		
		System.out.println(Arrays.toString(arr));
	}

}
