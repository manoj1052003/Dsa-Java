package com.jan26;

import java.util.Scanner;

public class class11num1 {
	
	public static int rev(int num) {
		int res = 0;
		while(num!=0) {
			int rem = num % 10;
			res = (res * 10) + rem;
			num = num / 10;
		}
		return res;
	}

	public static void main(String[] args) {
		int num = 454;
		int res = rev(num);
		System.out.println(res);
	
	}
}
