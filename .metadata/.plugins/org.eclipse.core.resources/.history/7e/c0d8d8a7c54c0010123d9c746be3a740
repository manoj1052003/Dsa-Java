package com.numbers;


public class classmeth {
	
	public static int sqr_digit(int num) {
		int res = 0;
		while(num != 0) {
			int rem = num % 10;
			res = (rem * rem ) + res;
			num = num / 10;
		}
		return res;
	}
	
	
	public static void main(String[] args) {
		int num = 18;
		int res = sqr_digit(num);
		System.out.println(res);
		
	}
}
