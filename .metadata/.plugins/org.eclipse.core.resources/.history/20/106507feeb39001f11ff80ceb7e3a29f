package com.june;

public class num6 {
	
	public static int check_happy(int num) {
		int sum = 0;
		while(num!=0) {
			int rem = num % 10;
			sum = (rem * rem) + sum;
			num = num / 10;
		}
		return sum;
	}

	public static void main(String[] args) {
		// TODO Auto-generated method stub
			int num = 69;
			int res = num;
			while(res!=1 && res!=4) {
				res = check_happy(res);
			}
			if(res==1) {
				System.out.println("its happy");
			}
			else {
				System.out.println("its not");
			}
	}

}
