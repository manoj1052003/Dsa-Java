package com.basicop;

import java.util.Arrays;
import java.util.Scanner;

public class Test {

	public static void main(String[] args) {
		int [] [] arr = {{2,3,4},{5,6,7},{6,7,9}};
		int l_sum = 0;
		int r_sum = 0;
		for(int i=0; i<arr.length; i++) {
