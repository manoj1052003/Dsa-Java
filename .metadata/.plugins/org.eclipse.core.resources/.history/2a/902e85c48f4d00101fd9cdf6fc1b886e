package com.numbers;


public class classmeth { 
	
	public static int  sum_sqr_digit(int num) {
		int res = 0;
		while(num!=0) {
			int rem = num % 10;
			res = (rem * rem) + res;
			num = num / 10;
		}
		return res;
	}
	
	public static void main(String[] args) {
		int num = 19;
		int res = sum_sqr_digit(num);
		while(res != 1 && res!= 4) {
			res = sum_sqr_digit(res);
		}
		if(res == 1) {
			System.out.println("happy");
		}
		if(res == 4) {
			System.out.println("not happy");
		}
		
	}
}
