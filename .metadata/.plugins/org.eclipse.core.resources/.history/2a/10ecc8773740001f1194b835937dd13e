package com.june;
import java.util.*;
public class array3 {
	
	public static int l_search(int[] arr, int key) {
		for(int i=0; i<arr.length; i++) {
			if(arr[i]==key) {
				return i;
			}
		}
		return -1;
	}
	
	
	public static int b_search(int[] arr, int key) {
		int low = 0;
		int high = arr.length-1;
		while(low<=high) {
			int mid = (low+high)/2;
			if(arr[mid]==key) {
				return mid;
			}
			else if(key>mid) {
				low = mid + 1;
			}
			else {
				high = mid - 1;
			}
		}
		return -1;
	}

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int [] arr = {12,23,34,45,56,67,78,89};
		int key = 67;
		int index = b_search(arr,key);
		System.out.println(index);
		
		
	}

}
