package com.opr;

import java.util.Arrays;

public class Pattern3 {
	
	public static void main(String[] args) {
		
		int [][] arr = {{1,2,3},
						{4,5,6},
						{9,8,9}}; 
		int sum1 = 0;
		int sum2 = 0;
		for(int i=0; i<arr.length; i++) {
			for(int j=0; j<arr.length; j++) {
				System.out.print(arr[i][j]+" ");
			}
		}
	}

}
