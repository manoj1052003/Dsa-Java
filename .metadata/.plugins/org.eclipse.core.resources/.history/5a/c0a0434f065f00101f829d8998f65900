	public static String f_upper(String word) {// class
		String res = "";
		for(int i=0; i<word.length();i++) {
			if(i==word.length()-1) {
				res = (char)(word.charAt(i)-32)+res;
			}
			else {
				res =  word.charAt(i)+res;
			}
		}
		return res;
	}
	 
