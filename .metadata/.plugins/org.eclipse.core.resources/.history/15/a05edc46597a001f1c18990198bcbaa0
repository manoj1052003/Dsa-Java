package com.grooom;

public class balpar {
	
	public static boolean check(String b) {
		char [] arr = b.toCharArray();
		char s [] = new char [arr.length];
		int top = -1;
		for(char c : arr) {
			if(c=='{'||c=='['||c=='(') {
				top++;
				s[top] = c;
			}
			if(c=='}' && s[top] == '{') {
				top--;
			}
			if(c==']' && s[top] == '[') {
				top--;
			}
			if(c==')' && s[top] == '(') {
				top--;
			}
		}
		if(top==-1) {
			return true;
		}
		return false; 
	}

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		String b = "{[{}]}";
		System.out.println(check(b));
	}

}
