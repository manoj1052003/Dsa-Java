	public static void main(String[] args) {
		
		int [][] arr = {{1,2,3},{4,5,6},{9,8,9}};
		int r_sum = 0;
		int l_sum = 0;
		for(int i=0; i<arr.length; i++) {
			for(int j=0; j<arr[0].length; j++) {
				if(i==j) {
					l_sum+=arr[i][j];
				}
				if(i+j==arr.length-1) {
					r_sum+=arr[i][j];
				}
			}
		}
		System.out.println(Math.abs(r_sum-l_sum));
		
		
	}
}
