package com.basics;

public class Class2 {

	public static void main(String[] args) {
		int rows = 5;
		int k = 1;
		for (int i = 1; i <= rows; i++) {
			for (int j = 1; j <= i; j++) {
				if(k%2==0) {
					System.out.print(k+ " ");
				}
				else {
					System.out.print(k+ " ");
				}
				
				k++;
			}
			System.out.println();
		}
	}
}
