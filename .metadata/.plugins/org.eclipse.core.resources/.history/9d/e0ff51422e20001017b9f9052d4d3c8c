package com.sepnov;

import java.util.Arrays;

public class num1 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int [] arr = { 12, 23, 34, 45, 56, 67, 78 };
		System.out.println(Arrays.toString(arr));
		int i = 0;
		int j = arr.length-1;
		while(i < j) {
			int temp = arr[j];
			arr[j] = arr[i];
			arr[i] = temp;
			i++;
			j--;
		}
		System.out.println(Arrays.toString(arr));

	}

}
