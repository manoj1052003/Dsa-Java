package com.basic;

import java.util.Arrays;

public class Class3 {

	public static void conv_chararray(String s) {
		char[] arr = new char[s.length()];
		for (int i = 0; i < s.length(); i++) {
			arr[i] = s.charAt(i);
		}
		System.out.println(Arrays.toString(arr));
	}

	public static void main(String[] args) {
		String s = "hello";
		conv_chararray(s);

	}

}
