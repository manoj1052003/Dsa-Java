package com.june19;

public class ninthclass {
	
	public static int reverse(int num) {
		int res = 0;
		while(num!=0) {
			int rem = num % 10;
			res = (res * 10) + rem;
			num = num / 10;
		}
		return res;
	}

	public static void main(String[] args) {
		int count = 0;
		int sum = 0;
		for(int i=1; ; i++) {
			int num = i;
			int rev = reverse(num);
			if( num == rev) {
				sum+=i;
				count++;
			}
			if(count==15) {
				System.out.println(sum);
				break;
			}

		}
		
		
	}
}
