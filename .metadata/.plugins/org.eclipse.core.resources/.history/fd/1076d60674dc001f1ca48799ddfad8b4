package com.novdecnew;

public class class14 {
	
	public static int happy(int num) {
		int res = 0;
		while(num!=0) {
			int rem = num % 10;
			res = (rem * rem) + res;
			num = num / 10;
		}
		return res;
	}

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int num = 18;
		int res = happy(num);
		System.out.println(res);
		
	}

}
