package com.pattern;

import java.util.Scanner;

public class pattern2 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int rows = 5;
		for(int i=1; i<=rows; i++) {
			for(int j=6-i; j<=5; j++) {
				System.out.print(j+" ");
			}
			System.out.println();
		}
	}

}
