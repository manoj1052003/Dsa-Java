package com.june19;

public class thirteen {
	
	public static int rec_fibo(int num) {
		if(num<=1) {
			return 0 ;
		}
		if(num==2) {
			return 1;
		}
		
		return rec_fibo(num-1)+rec_fibo(num-2);
		
	}
	
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int num = 8;
		int sum = rec_fibo(num);
		System.out.println(sum);
		
	}

}
