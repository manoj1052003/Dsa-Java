package com.jan26;

import java.util.Arrays;
import java.util.Scanner;

public class class14num4 {

	public static void main(String[] args) {
		float sum = 0;
		int count = 0;
		int [] arr = {12,23,34,45,56,67,78};
		for(int i=0; i<arr.length; i++) {
			if(arr[i]%2!=0) {
				sum = sum + arr[i];
				count++;
			}
			
		}
		System.out.println(sum/count);
	}

}
