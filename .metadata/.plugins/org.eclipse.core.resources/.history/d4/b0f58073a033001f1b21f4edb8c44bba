package com.pattern;

public class impresscrush {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		int rows = 6;
		int col = 7;
		
		for (int i =0; i<rows; i++) {
			for (int j = 0; j < col; j++) {	
				if ((i==4 && j%3==0) || (i==5 && j%3!=0) || (i+j==3) || (j-i==3)){	
					System.out.print("*"+" ");
				}
				else if(i==3 && j==2) {
					System.out.print("R"+" ");
				}
				else if(i==3 && j==3) {
					System.out.print("C"+" ");
				}
				else if(i==3 && j==4) {
					System.out.print("B"+" ");
				}
				else {
					
					System.out.print(" "+" ");
				}	
			}
			System.out.println();
		}
	
	}

}
