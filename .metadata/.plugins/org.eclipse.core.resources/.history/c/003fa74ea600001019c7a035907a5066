package com.jan26;

import java.util.Scanner;

public class class11num1 {
	
	

	public static void main(String[] args) {
		int [] arr1 = {2,4,8,14};
		int [] arr2 = {1,3,5};
		int [] res = new int [arr1.length+arr2.length];
		int i = 0;
		int j = 0;
		int k = 0;
		
		if(arr1[i]<arr2[j]) {
			res[k] = arr1[i];
		}
		else {
			res[k] = arr2[j];
		}
		
		
	}
}
