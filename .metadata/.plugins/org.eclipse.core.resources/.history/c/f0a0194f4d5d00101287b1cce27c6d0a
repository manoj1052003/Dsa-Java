package com.basic;

public class Logicalopr {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int a = 10;
		int b = 20;
		System.out.println((a==a) && (a!=b));
		System.out.println((a==a) && (a!=a));
		System.out.println((a!=a) && (a>b));
		System.out.println((a==a) || (a!=b));
		System.out.println((a==a) || (a!=a));
		System.out.println((a!=a) || (a>b));
		System.out.println(10==10;
		
		
	}

}
