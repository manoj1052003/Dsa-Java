package com.jan26;

public class class14num4 {
	
	public static int b_search(int [] arr, int key) {
		int low = 0;
		int high = arr.length-1;
		while(low<=high) {
			int mid = (low+high)/2;
			if(arr[mid] == key) {
				return mid;
			}
			else if(key > arr[mid]) {
				low = mid + 1;
			}
			else {
				high = mid -1;
			}
		}
		return -1;
	}
	
	public static void main(String[] args) {
		int arr [] = {12,23,34,45,56,67,78};
		int key = 23;
		int ind = b_search(arr, key);
		System.out.println(ind);
	}

}
