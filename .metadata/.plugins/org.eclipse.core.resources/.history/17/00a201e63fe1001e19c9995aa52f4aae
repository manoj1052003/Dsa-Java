package com.csr;

public class assignemnt {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
			int rows=5;
			int k = 65;
			for (int i=1; i<=rows;i++) {
				for (int j=1; j<=(rows-i);j++) {
					System.out.print(" "+" ");
				}
				char c = (char)k;
				for (int j=1; j<=i; j++) {
					System.out.print(k+" ");
				}
				System.out.println();
			}
	}

}
