package com.april;
import java.util.*;

public class pattern1 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner sc = new Scanner(System.in);
		int rows = sc.nextInt();
		for (int i = 1; i<=rows; i++) {
			for (int j=1; j<=i ; j++) {
				System.out.print((i+j-1)+" ");
			}
			System.out.println();
		}
	}

}
