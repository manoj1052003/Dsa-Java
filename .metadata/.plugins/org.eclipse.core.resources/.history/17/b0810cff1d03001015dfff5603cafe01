package com.jan26;

public class class14num4 {
	
	public static int c_digit(int num) {
		int digit = 0;
		while(num!=0) {
			num = num / 10;
			digit++;
		}
		return digit;
	}

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int num = 164;
		int digit = c_digit(num);
		System.out.println(digit);
	}

}
