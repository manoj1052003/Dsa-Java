package com.sepnov;

public class Lastclass {
	
	
	public static void find_ans(String s) {
		for(int i=0; i<s.length(); i+=2) {
			char ch = s.charAt(i+1);
			System.out.println(ch-2);
			break;
		}
		
	}

	public static void main(String[] args) {
		String s = "a2b5c3";
		find_ans(s);
		
		
		

	}

}
