package com.sepnov;

import java.util.Arrays;

public class num1 {

	public static void main(String[] args) {
		int [] arr = {-5,2,7,-4,12,-1};
		for(int i=0; i<arr.length; i++) {
			for(int j=i; j<arr.length; j++) {
				int sum = 0;
				for(int k=i; k<=j; k++) {
					sum +=arr[k];
				}
				System.out.println(sum);
			}
		}
		

	}

}
