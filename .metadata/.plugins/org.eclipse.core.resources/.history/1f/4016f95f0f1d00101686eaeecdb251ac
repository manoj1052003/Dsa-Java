package com.april;

public class armstrong {
	
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int rows = 5;
		for(int i=1; i<=rows; i++) {
			for(int j=rows+1-i; j>=1; j--) {
				System.out.print((char)(j+64)+" ");
			}
			System.out.println();
		}
		for(int i=rows-1; i>=1; i--) {
			for(int j=rows+1-i; j>=1; j--) {
				System.out.print((char)(j+64)+" ");
			}
			System.out.println();
		}
		
		
		
		
	}

}
