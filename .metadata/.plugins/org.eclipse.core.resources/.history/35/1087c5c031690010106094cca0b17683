package com.basicop;

import java.util.Arrays;
import java.util.Scanner;

public class Class1assign {

	public static void main(String[] args) {
		int [] arr = {12,23,34,45,56,67,78,89};
		float sum = 0;
		for(int i=0; i<arr.length; i++) {
			sum+=arr[i];
		}
		System.out.println(sum/arr.length);
	}

}
