package com.sepoct;
import java.util.*;
public class num1 {
	
	public static int digit_sum(int num) {
		int sum = 0;
		while(num!=0) {  //0
			int rem =  num % 10;
			sum = sum + rem;  //5
			num = num / 10;
		}
		return sum;
	}
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int num = 99;
		int res = num + 1;
		while(digit_sum(num)*2!=digit_sum(res)) {
			res++;
		}
		System.out.println(res);
		
		
	}
}
