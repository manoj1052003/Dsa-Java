package com.basic;

public class Logicalopr {
	
	public static String toUpper(String s) {
		String res = "";
		for(int i=0; i<s.length();i++) {
			char ch = s.charAt(i);
			if(ch >= 65 && ch <= 90) {
				char nch = (char)(ch + 32);
				res += (nch);
			}
			else {
				char nch = (char)(ch - 32);
				res += (nch);
			}
		}
		return res;
	}
	
	 
	public static void main(String[] args) {
	
		String s = "pENtagON";
		String res = toUpper(s);
		System.out.println(res);
	}

}
