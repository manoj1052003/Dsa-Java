package com.basicspro;

import java.util.Arrays;

public class firdtclass {
	
	public static void main(String[] args) {
		String s = "HeLlo";
		String res = "";
		for(int i=0; i<s.length(); i++) {
			char ch = s.charAt(i);
			if(ch >=65 && ch<=90) {
				res = res + (char)(ch+32);
			}
			else {
				res = res + (char)(ch-32);
			}
		}
		System.out.println(res);
	}
}
	
