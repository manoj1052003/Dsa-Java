package com.numbers;

public class classmeth {
	
	public static void reverse(int num) {
		int res = 0;
		int temp = num;
		while (num != 0) {
			int rem = num % 10;
			res = (res * 10) + rem;
			num = num / 10;
		}
		System.out.println(res);
	}

	public static void main(String[] args) {
		int num = 123;
		reverse(num);
//		System.out.println(temp);
//		System.out.println(res);
//		if(temp == res) {
//			System.out.println("its palindrome");
//		}
//		else {
//			System.out.println("its not");
//		}
	}
}
