package com.basics;

public class Class2 {

	public static void main(String[] args) {
		int [] arr = {1,2,4,5,7};
		for(int i=0; i<arr.length; i++) {
			for(int j=i; j<arr.length; j++) {
				for(int k=i; k<=j; k++) {
					System.out.print(arr[k]+" ");
				}
				System.out.println();
			}
		}
	
	}
}
