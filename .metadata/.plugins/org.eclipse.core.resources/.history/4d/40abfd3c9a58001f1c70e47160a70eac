package com.june;
import java.util.Arrays;
public class array1 {
	
	public static int add_ele(int [] arr) {
		// start here
		int max = arr[0];
		for(int val : arr) {
			 if(val>max) {
				 max=val;
			 }
		}
		return max;
	}

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int [] arr = {-2333,-45,-566,-78,-90,-12};
		int sum = add_ele(arr);
		System.out.println(sum);
	}
}
