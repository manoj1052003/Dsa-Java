package com.pattern;

import java.util.*;

public class secondpro {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		int rows = 5;
		
		int k = 1;
		
		int [][] arr = new int [rows][rows];
		
		
		int low = 0;
		int high = rows;
		for(int i=1; i<=rows; i++) {
			for (int j=1; j<=rows; j++) {
				arr[low][j]=k;
				k++;
			}
		}
		System.out.print(Arrays.deepToString(arr));
	}
}
