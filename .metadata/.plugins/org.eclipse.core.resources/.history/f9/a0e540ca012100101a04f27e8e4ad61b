package com.sepnov;

public class num1 {

	public static int b_search(int arr[], int key) {
		int low = 0;
		int high = arr.length - 1;
		while (low <= high) {
			int mid = (low + high) / 2;
			if (key == arr[mid]) {
				return mid;
			} else if (key > arr[mid]) {
				low = mid + 1;
			} else {
				high = mid - 1;
			}
		}
		return -1;
	}

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int[] arr = { 12, 23, 34, 45, 56, 67 };
		int key = 35;
		System.out.println(b_search(arr, key));

	}

}
